<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='imp_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='imp_codigo' rule='contains' />
	<parameter id='imp_descripcion' type='varchar' required='false' label='${lbl:b_description}' maxlen='48'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='imp_descripcion' rule='contains' />
	<parameter id='imp_tipo_persona' type='varchar' required='false' label='${lbl:b_kind_of_person}' maxlen='2'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='imp_tipo_persona' rule='like' />
	<parameter id='imp_tipo_contribuyente' type='varchar' required='false' label='${lbl:b_type_of_taxpayer}' maxlen='2'/>
	<parameter id='imp_unidad_impositiva' type='integer' required='false' label='${lbl:b_tax_unit}' maxlen='10'/>
	<parameter id='imp_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date_grater_than}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date_grater_than} mayor o igual que] no puede ser mayor que [${lbl:b_store_date_less_than} menor o igual que].' date1='imp_fecha_registro_ini' date2='imp_fecha_registro_fin' id='imp_fecha_registro_ini' />
	<parameter id='imp_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date_less_than}' maxlen='10'/>
</validator>
