<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnpaneltraduccion.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
	
	<excel recordset='query-smnpaneltraduccion.sql' sheetname='${lbl:b_panel_translation}' filename='smn_panel_traduccion.xls'>
		<col id='pat_panel' label='${lbl:b_service_description}{select ${schema}s_service.service_id as id, ${schema}s_service.description || ' - ' || ${schema}s_service.path as item from ${schema}s_service order by ${schema}s_service.description}' />
		<col id='pat_description' label='${lbl:b_description}' />
		<col id='pat_locale_combo' label='${lbl:b_idiom}' />
		<col id='pat_fecha_registro' label='${lbl:b_store_date}' />
	</excel>

</config>