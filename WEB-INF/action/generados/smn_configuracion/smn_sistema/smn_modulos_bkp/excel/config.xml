<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnmodulos.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
	
	<excel recordset='query-smnmodulos.sql' sheetname='${lbl:b_modules}' filename='smn_modulos.xls'>
		<col id='ent_codigo_pl0' label='${lbl:b_entity}' />
		<col id='smn_app_id' label='${lbl:s_loginhist_col4}' />
		<col id='mod_codigo' label='${lbl:b_code}' />
		<col id='mod_nombre' label='${lbl:b_name}' />
		<col id='mod_fecha_registro' label='${lbl:b_store_date}' />
	</excel>

</config>