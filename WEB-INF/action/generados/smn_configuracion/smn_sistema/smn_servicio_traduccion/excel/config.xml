<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnserviciotraduccion.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
	
	<excel recordset='query-smnserviciotraduccion.sql' sheetname='${lbl:b_service_translation}' filename='smn_servicio_traduccion.xls'>
		<col id='set_app_id' label='${lbl:b_application}' />
		<col id='set_menu_id' label='${lbl:b_menu}' />
		<col id='set_servicio_id' label='${lbl:b_service}' />
		<col id='set_servicio' label='${lbl:b_service_title}' />
		<col id='set_locale_combo' label='${lbl:b_idiom}' />
		<col id='set_fecha_registro' label='${lbl:b_store_date}' />
	</excel>

</config>