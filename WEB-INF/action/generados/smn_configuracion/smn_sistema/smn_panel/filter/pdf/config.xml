<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnpanel.sql" source="session" scope="transaction" />
		<recordset id="filter.params" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lbl:b_panel}</pdf-title>
	<pdf-logo url='/images/logo-dinamica.png' scale='100' />

	<pdf-report pageSize="letter" rotate="false">
		<record recordset='filter.params' title='Filtro de Búsqueda' noFilterMsg='-- Sin filtro de búsqueda --' width='55' spacingBefore='15'>
			<col name='pan_service_description' title='${lbl:b_service_description}'  align='center' />
			<col name='pan_app_id' title='${lbl:b_application}'  format='######' align='center' />
			<col name='pan_role_id' title='${lbl:b_role}'  format='######' align='center' />
			<col name='pan_fecha_registro_ini' title='${lbl:b_store_date_grater_than}'  format='dd-MM-yyyy' align='center' />
			<col name='pan_fecha_registro_fin' title='${lbl:b_store_date_less_than}'  format='dd-MM-yyyy' align='center' />
		</record>
		<table recordset='query-smnpanel.sql' width='100' spacingBefore='10'>
			<col name='pan_service_description' title='${lbl:b_service_description}' width='14' align='left' />
			<col name='pan_service_path' title='${lbl:b_service_path}' width='14' align='left' />
			<col name='pan_icon_path' title='${lbl:b_icon_path}' width='14' align='left' />
			<col name='pan_app_id' title='${lbl:b_application}' width='14' align='left' format='######' />
			<col name='pan_role_id' title='${lbl:b_role}' width='14' align='left' format='######' />
			<col name='pan_orden' title='${lbl:b_order}' width='14' align='left' format='######' />
			<col name='pan_fecha_registro' title='${lbl:b_store_date}' width='14' align='center' format='dd-MM-yyyy' />
		</table>

	</pdf-report>
</config>