<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smntipocuentabanco.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
	
	<excel recordset='query-smntipocuentabanco.sql' sheetname='${lbl:b_types_of_bank_accounts}' filename='smn_tipo_cuenta_banco.xls'>
		<col id='tcb_codigo' label='${lbl:b_code}' />
		<col id='tcb_descripcion' label='${lbl:b_description}' />
		<col id='tcb_fecha_registro' label='${lbl:b_store_date}' />
	</excel>

</config>