<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smncondicionpago.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
	
	<excel recordset='query-smncondicionpago.sql' sheetname='${lbl:b_condition_payments}' filename='filter.xls'>
		<col id='cop_codigo' label='${lbl:b_code}' />
		<col id='cop_descripcion' label='${lbl:b_description}' />
		<col id='cop_fecha_registro' label='${lbl:b_store_date}' />
	</excel>

</config>