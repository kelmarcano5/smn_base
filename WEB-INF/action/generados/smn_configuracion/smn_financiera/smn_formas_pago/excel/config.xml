<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnformaspago.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
	
	<excel recordset='query-smnformaspago.sql' sheetname='${lbl:b_way_to_pay}' filename='smn_formas_pago.xls'>
		<col id='fop_codigo' label='${lbl:b_code}' />
		<col id='fop_descripcion' label='${lbl:b_description}' />
		<col id='fop_medio_pago' label='${lbl:b_medio_pago}' />
		<col id='fop_requiere_banco_combo' label='${lbl:b_bank}' />
		<col id='fop_franquicia' label='${lbl:b_franquicias}' />
		<col id='fop_fecha_registro' label='${lbl:b_store_date}' />
	</excel>

</config>