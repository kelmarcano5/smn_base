<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='prs_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='prs_codigo' rule='contains' />
	<parameter id='prs_nombres' type='varchar' required='false' label='${lbl:s_profile_fname}' maxlen='32'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='prs_nombres' rule='contains' />
	<parameter id='prs_apellidos' type='varchar' required='false' label='${lbl:s_profile_lname}' maxlen='32'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='prs_apellidos' rule='contains' />
	<parameter id='prs_area_servicio' type='integer' required='false' label='${lbl:b_service_area}' maxlen='10'/>
	<parameter id='prs_unidad_servicio' type='integer' required='false' label='${lbl:b_service_unit}' maxlen='10'/>
	<parameter id='prs_auxiliar' type='integer' required='false' label='${lbl:b_auxiliary}' maxlen='10'/>
	<parameter id='prs_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date_grater_than}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date_grater_than} mayor o igual que] no puede ser mayor que [${lbl:b_store_date_less_than} menor o igual que].' date1='prs_fecha_registro_ini' date2='prs_fecha_registro_fin' id='prs_fecha_registro_ini' />
	<parameter id='prs_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date_less_than}' maxlen='10'/>
</validator>
