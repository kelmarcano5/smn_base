<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='cmp_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='cmp_codigo' rule='contains' />
	<parameter id='cmp_descripcion' type='varchar' required='false' label='${lbl:b_description}' maxlen='48'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='cmp_descripcion' rule='contains' />
	<parameter id='cmp_area_servicio' type='integer' required='false' label='${lbl:b_service_area}' maxlen='10'/>
	<parameter id='cmp_unidad_servicio' type='integer' required='false' label='${lbl:b_service_unit}' maxlen='10'/>
	<parameter id='cmp_tipo' type='varchar' required='false' label='${lbl:b_type}' maxlen='2'/>
	<parameter id='cmp_trans_general' type='integer' required='false' label='${lbl:b_general_transaction}' maxlen='10'/>
	<parameter id='cmp_vigencia_desde' type='date' required='false' label='${lbl:b_validation_date_from}' maxlen='10'/>
	<parameter id='cmp_vigencia_hasta' type='date' required='false' label='${lbl:b_validation_date_to}' maxlen='10'/>
	<parameter id='cmp_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date_grater_than}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date_grater_than} mayor o igual que] no puede ser mayor que [${lbl:b_store_date_less_than} menor o igual que].' date1='cmp_fecha_registro_ini' date2='cmp_fecha_registro_fin' id='cmp_fecha_registro_ini' />
	<parameter id='cmp_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date_less_than}' maxlen='10'/>
</validator>
