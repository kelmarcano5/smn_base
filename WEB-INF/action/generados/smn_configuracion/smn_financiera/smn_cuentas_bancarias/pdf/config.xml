<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smncuentasbancarias.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lbl:b_bank_accounts}</pdf-title>
	<pdf-logo url='/images/logo-dinamica.png' scale='100' />

	<pdf-report pageSize="letter" rotate="false">
		<table recordset='query-smncuentasbancarias.sql' width='100' spacingBefore='40'>
			<col name='cba_entidades_financieras' title='${lbl:b_financial_entity}' width='14' align='center' format='######' />
			<col name='cba_codigo' title='${lbl:b_code}' width='14' align='left' />
			<col name='cba_nombre' title='${lbl:b_name}' width='14' align='left' />
			<col name='cba_nro_cuenta' title='${lbl:b_bank_account_number}' width='14' align='left' />
			<col name='cba_tipo_cta_banco' title='${lbl:b_account_bank_type}' width='14' align='center' format='######' />
			<col name='cba_moneda' title='${lbl:b_coin}' width='14' align='center' format='######' />
			<col name='cba_fecha_registro' title='${lbl:b_store_date}' width='14' align='center' format='dd-MM-yyyy' />
		</table>

	</pdf-report>
</config>