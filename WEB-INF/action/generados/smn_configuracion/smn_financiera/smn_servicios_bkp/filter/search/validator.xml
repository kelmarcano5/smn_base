<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='svc_descripcion' type='varchar' required='false' label='${lbl:b_description}' maxlen='56'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='svc_descripcion' rule='contains' />
	<parameter id='svc_clase_auxiliar' type='integer' required='false' label='${lbl:b_auxiliary_class}' maxlen='10'/>
	<parameter id='svc_tipo_servicio' type='integer' required='false' label='${lbl:b_service_type}' maxlen='10'/>
	<parameter id='svc_area_servicio' type='integer' required='false' label='${lbl:b_service_area}' maxlen='10'/>
	<parameter id='svc_unidad_servicio' type='integer' required='false' label='${lbl:b_service_unit}' maxlen='10'/>
	<parameter id='svc_inactivo' type='varchar' required='false' label='${lbl:b_inactive}' maxlen='1'/>
	<parameter id='svc_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date_grater_than}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date_grater_than} mayor o igual que] no puede ser mayor que [${lbl:b_store_date_less_than} menor o igual que].' date1='svc_fecha_registro_ini' date2='svc_fecha_registro_fin' id='svc_fecha_registro_ini' />
	<parameter id='svc_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date_less_than}' maxlen='10'/>
</validator>
