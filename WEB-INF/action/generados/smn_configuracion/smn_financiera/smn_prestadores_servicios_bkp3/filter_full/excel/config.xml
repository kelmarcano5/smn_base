<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnprestadoresservicios.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
	
	<excel recordset='query-smnprestadoresservicios.sql' sheetname='${lbl:b_services_providers}' filename='filter_full.xls'>
		<col id='prs_codigo' label='${lbl:b_code}' />
		<col id='prs_nombres' label='${lbl:s_profile_fname}' />
		<col id='prs_apellidos' label='${lbl:s_profile_lname}' />
		<col id='prs_area_servicio' label='${lbl:b_service_area}' />
		<col id='prs_unidad_servicio' label='${lbl:b_service_unit}' />
		<col id='prs_clase_auxiliar' label='${lbl:b_auxiliary_class}' />
		<col id='prs_auxiliar' label='${lbl:b_auxiliary}' />
		<col id='smn_grupos_prestadores' label='${lbl:b_group_of_provider}' />
		<col id='prs_direccion' label='${lbl:b_address}' />
		<col id='prs_telefono_fijo' label='${lbl:b_fixed_phone}' />
		<col id='prs_telefono_movil' label='${lbl:b_movile_phone}' />
		<col id='prs_frecuencia' label='${lbl:b_frecuency}' />
		<col id='prs_fecha_registro' label='${lbl:b_store_date}' />
	</excel>

</config>