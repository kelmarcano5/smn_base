<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smncomponentes.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
	
	<excel recordset='query-smncomponentes.sql' sheetname='${lbl:b_components}' filename='smn_componentes.xls'>
		<col id='gpp_codigo_pl0' label='${lbl:b_group_of_provider}' />
		<col id='svc_codigo_pl1' label='${lbl:b_service}' />
		<col id='cmp_descripcion' label='${lbl:b_description}' />
		<col id='cmp_area_servicio' label='${lbl:b_service_area}' />
		<col id='cmp_clase_auxiliar' label='${lbl:b_auxiliary_class}' />
		<col id='cmp_tipo_combo' label='${lbl:b_type}' />
		<col id='cmp_fecha_registro' label='${lbl:b_store_date}' />
	</excel>

</config>