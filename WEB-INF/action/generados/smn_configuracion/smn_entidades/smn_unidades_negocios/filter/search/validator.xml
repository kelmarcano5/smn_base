<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='uen_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='uen_codigo' rule='contains' />
	<parameter id='uen_nombre' type='varchar' required='false' label='${lbl:b_name}' maxlen='48'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='uen_nombre' rule='contains' />
	<parameter id='uen_responsable' type='varchar' required='false' label='${lbl:b_responsible}' maxlen='48'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='uen_responsable' rule='contains' />
	<parameter id='uen_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date_grater_than}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date_grater_than} mayor o igual que] no puede ser mayor que [${lbl:b_store_date_grater_than} menor o igual que].' date1='uen_fecha_registro_ini' date2='uen_fecha_registro_fin' id='uen_fecha_registro_ini' />
	<parameter id='uen_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date_grater_than}' maxlen='10'/>
</validator>
