<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnsucursales.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lbl:b_subsidiaries}</pdf-title>
	<pdf-logo url='/images/logo-dinamica.png' scale='100' />

	<pdf-report pageSize="letter" rotate="false">
		<table recordset='query-smnsucursales.sql' width='100' spacingBefore='40'>
			<col name='ent_codigo_pl0' title='${lbl:b_entity}' width='14' align='left' />
			<col name='suc_codigo' title='${lbl:b_code}' width='14' align='left' />
			<col name='suc_nombre' title='${lbl:b_name}' width='14' align='left' />
			<col name='suc_responsable' title='${lbl:b_responsible}' width='14' align='left' />
			<col name='suc_direccion' title='${lbl:b_address}' width='14' align='left' format='######' />
			<col name='suc_telefono_fijo' title='${lbl:b_fixed_phone}' width='14' align='left' />
			<col name='suc_fecha_registro' title='${lbl:b_store_date}' width='14' align='center' format='dd-MM-yyyy' />
		</table>

	</pdf-report>
</config>