<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lbl:b_entities}</pdf-title>
	<pdf-logo url='/images/logo-dinamica.png' scale='100' />

	<pdf-report pageSize="letter" rotate="false">
		<record recordset='query.sql' title='${lbl:b_entities}' width='60' spacingBefore='40'>
			<col name='ent_codigo' title='${lbl:b_code}' align='left' />
			<col name='ent_comunidad' title='${lbl:b_community}' align='left' format='######' />
			<col name='ent_corporacion' title='${lbl:b_corporation}' align='left' format='######' />
			<col name='ent_moneda' title='${lbl:b_currency}' align='left' format='######' />
			<col name='ent_sector_economico' title='${lbl:b_economic_sector}' align='left' format='######' />
			<col name='ent_descripcion_corta' title='${lbl:b_short_description}' align='left' />
			<col name='ent_descripcion_larga' title='${lbl:b_long_description}' align='left' />
			<col name='ent_nro_id_fiscal' title='${lbl:b_tax_identification_number}' align='left' />
			<col name='ent_direccion' title='${lbl:b_address}' align='left' format='######' />
			<col name='ent_contacto' title='${lbl:b_contact_person}' align='left' />
			<col name='ent_telefono_fijo' title='${lbl:b_fixed_phone}' align='left' />
			<col name='ent_telefono_movil' title='${lbl:b_mobile_phone}' align='left' />
			<col name='ent_email' title='${lbl:s_profile_email}' align='left' />
			<col name='ent_act_economica' title='${lbl:b_economic_activity}' align='left' />
			<col name='ent_fecha_registro' title='${lbl:b_store_date}' align='left' format='dd-MM-yyyy' />
		</record>

	</pdf-report>
</config>