<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smncorporaciones.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
	
	<excel recordset='query-smncorporaciones.sql' sheetname='${lbl:b_corporations}' filename='smn_corporaciones.xls'>
		<col id='crp_codigo' label='${lbl:b_code}' />
		<col id='crp_nombre' label='${lbl:b_name}' />
		<col id='crp_unidad_negocio' label='${lbl:b_business_unit}' />
		<col id='crp_moneda' label='${lbl:b_coin}' />
		<col id='crp_direccion' label='${lbl:b_address}' />
		<col id='crp_fecha_registro' label='${lbl:b_store_date}' />
	</excel>

</config>