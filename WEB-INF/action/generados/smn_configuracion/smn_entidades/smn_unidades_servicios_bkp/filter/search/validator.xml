<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_areas_servicios_id_name' type='varchar' required='false' label='${lbl:b_service_area}'/>
	<parameter id='smn_areas_servicios_id' type='integer' required='false' label='${lbl:b_service_area}' maxlen='10'/>
	<parameter id='uns_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='uns_codigo' rule='contains' />
	<parameter id='uns_descripcion' type='varchar' required='false' label='${lbl:b_description}' maxlen='48'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='uns_descripcion' rule='contains' />
	<parameter id='uns_centro_costo' type='integer' required='false' label='${lbl:b_cost_centers}' maxlen='10'/>
	<parameter id='uns_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date_grater_than}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date_grater_than} mayor o igual que] no puede ser mayor que [${lbl:b_store_date_less_than} menor o igual que].' date1='uns_fecha_registro_ini' date2='uns_fecha_registro_fin' id='uns_fecha_registro_ini' />
	<parameter id='uns_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date_less_than}' maxlen='10'/>
</validator>
