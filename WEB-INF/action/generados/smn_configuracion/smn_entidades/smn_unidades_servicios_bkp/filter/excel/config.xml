<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnunidadesservicios.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
	
	<excel recordset='query-smnunidadesservicios.sql' sheetname='${lbl:b_service_units}' filename='filter.xls'>
		<col id='ase_codigo_pl0' label='${lbl:b_service_area}' />
		<col id='uns_codigo' label='${lbl:b_code}' />
		<col id='uns_descripcion' label='${lbl:b_description}' />
		<col id='uns_centro_costo' label='${lbl:b_cost_centers}' />
		<col id='uns_fecha_registro' label='${lbl:b_store_date}' />
	</excel>

</config>