<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smncentrocosto.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
	
	<excel recordset='query-smncentrocosto.sql' sheetname='${lbl:b_cost_centers}' filename='smn_centros_costos.xls'>
		<col id='ent_codigo_pl0' label='${lbl:b_entity}' />
		<col id='cco_codigo' label='${lbl:b_code}' />
		<col id='cco_descripcion_corta' label='${lbl:b_short_description}' />
		<col id='cco_responsable' label='${lbl:b_responsible}' />
		<col id='cco_clase_auxiliar' label='${lbl:b_auxiliary_class}' />
		<col id='cco_auxiliar' label='${lbl:b_auxiliary}' />
		<col id='cco_fecha_registro' label='${lbl:b_store_date}' />
	</excel>

</config>