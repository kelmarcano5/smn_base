<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_entidades_id_name' type='varchar' required='false' label='${lbl:b_entity}'/>
	<parameter id='smn_entidades_id' type='integer' required='false' label='${lbl:b_entity}' maxlen='10'/>
	<parameter id='cco_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='cco_codigo' rule='contains' />
	<parameter id='cco_descripcion_corta' type='varchar' required='false' label='${lbl:b_short_description}' maxlen='24'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='cco_descripcion_corta' rule='contains' />
	<parameter id='cco_responsable' type='varchar' required='false' label='${lbl:b_responsible}' maxlen='48'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='cco_responsable' rule='contains' />
	<parameter id='cco_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date_grater_than}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date_grater_than} mayor o igual que] no puede ser mayor que [${lbl:b_store_date_less_than} menor o igual que].' date1='cco_fecha_registro_ini' date2='cco_fecha_registro_fin' id='cco_fecha_registro_ini' />
	<parameter id='cco_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date_less_than}' maxlen='10'/>
</validator>
