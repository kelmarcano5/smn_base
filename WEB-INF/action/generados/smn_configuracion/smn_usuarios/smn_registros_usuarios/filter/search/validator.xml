<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='rgu_tipo_doc_oficial' type='integer' required='false' label='${lbl:b_official_document_type}' maxlen='16'/>
	<parameter id='rgu_num_doc_oficial' type='varchar' required='false' label='${lbl:b_official_document_number}' maxlen='12'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='rgu_num_doc_oficial' rule='contains' />
	<parameter id='rgu_nro_id_fiscal' type='varchar' required='false' label='${lbl:b_tax_identification_number}' maxlen='16'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='rgu_nro_id_fiscal' rule='contains' />
	<parameter id='rgu_nombres' type='varchar' required='false' label='${lbl:s_profile_fname}' maxlen='32'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='rgu_nombres' rule='contains' />
	<parameter id='rgu_apellidos' type='varchar' required='false' label='${lbl:s_profile_lname}' maxlen='32'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='rgu_apellidos' rule='contains' />
	<parameter id='rgu_tipo_registro' type='integer' required='false' label='${lbl:b_register_type}' maxlen='10'/>
	<parameter id='rgu_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date_grater_than}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date_grater_than} mayor o igual que] no puede ser mayor que [${lbl:b_store_date_less_than} menor o igual que].' date1='rgu_fecha_registro_ini' date2='rgu_fecha_registro_fin' id='rgu_fecha_registro_ini' />
	<parameter id='rgu_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date_less_than}' maxlen='10'/>
</validator>
