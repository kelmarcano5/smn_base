<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_terceros_id_name' type='varchar' required='false' label='${lbl:b_outsourced_company}'/>
	<parameter id='smn_terceros_id' type='integer' required='false' label='${lbl:b_outsourced_company}' maxlen='10'/>
	<parameter id='tru_comunidad' type='integer' required='false' label='${lbl:b_community}' maxlen='10'/>
	<parameter id='tru_nombres' type='varchar' required='false' label='${lbl:s_profile_fname}' maxlen='32'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='tru_nombres' rule='contains' />
	<parameter id='tru_apellidos' type='varchar' required='false' label='${lbl:s_profile_lname}' maxlen='32'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='tru_apellidos' rule='contains' />
	<parameter id='tru_nro_doc_identidad' type='varchar' required='false' label='${lbl:b_identification_doc_number}' maxlen='16'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='tru_nro_doc_identidad' rule='contains' />
</validator>
