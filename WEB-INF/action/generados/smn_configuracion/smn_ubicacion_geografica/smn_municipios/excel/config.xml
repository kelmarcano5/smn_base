<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnmunicipios.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
	
	<excel recordset='query-smnmunicipios.sql' sheetname='${lbl:b_municipalities}' filename='smn_municipios.xls'>
		<col id='mun_ciudades' label='${lbl:b_city}' />
		<col id='mun_codigo' label='${lbl:b_code}' />
		<col id='mun_nombre_corto' label='${lbl:b_short_name}' />
		<col id='mun_nombre_largo' label='${lbl:b_long_name}' />
		<col id='mun_fecha_registro' label='${lbl:b_store_date}' />
	</excel>

</config>