<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnzonaspostales.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
	
	<excel recordset='query-smnzonaspostales.sql' sheetname='${lbl:b_postal_zones}' filename='smn_zonas_postales.xls'>
		<col id='zpo_parroquias_combo' label='${lbl:b_parish}' />
		<col id='zpo_sector' label='${lbl:b_sector}' />
		<col id='zpo_codigo' label='${lbl:b_code}' />
		<col id='zpo_fecha_registro' label='${lbl:b_store_date}' />
	</excel>

</config>