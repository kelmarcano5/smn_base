<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnauxiliarareasservicios.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lbl:b_service_areas}</pdf-title>
	<pdf-logo url='/images/logo-dinamica.png' scale='100' />

	<pdf-report pageSize="letter" rotate="false">
		<table recordset='query-smnauxiliarareasservicios.sql' width='100' spacingBefore='40'>
			<col name='tts_codigo_pl0' title='${lbl:b_outsourced_type_of_service}' width='14' align='left' />
			<col name='tse_codigo' title='${lbl:b_code}' width='14' align='left' />
			<col name='tse_descripcion' title='${lbl:b_description}' width='14' align='left' />
			<col name='tse_auxiliar' title='${lbl:b_auxiliary}' width='14' align='left' format='######' />
			<col name='tse_auxiliar_sucursal' title='${lbl:b_outsourced_subsidiary}' width='14' align='left' format='######' />
			<col name='tse_auxiliar_unidad_negocio' title='${lbl:b_outsourced_unit_business}' width='14' align='left' format='######' />
			<col name='tse_fecha_registro' title='${lbl:b_store_date}' width='14' align='center' format='dd-MM-yyyy' />
		</table>

	</pdf-report>
</config>