<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnauxiliarsucursales.sql" source="session" scope="transaction" />
		<recordset id="filter.params" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lblb_outsourced_subsidiaries}</pdf-title>
	<pdf-logo url='/images/logo-dinamica.png' scale='100' />

	<pdf-report pageSize="letter" rotate="false">
		<record recordset='filter.params' title='Filtro de Búsqueda' noFilterMsg='-- Sin filtro de búsqueda --' width='55' spacingBefore='15'>
			<col name='trs_codigo' title='${lbl:b_code}'  align='center' />
		</record>
		<table recordset='query-smnauxiliarsucursales.sql' width='100' spacingBefore='10'>
			<col name='trs_codigo' title='${lbl:b_code}' width='11' align='left' />
			<col name='trs_nombre' title='${lbl:b_name}' width='11' align='left' />
			<col name='trs_auxiliar_unidad_negocio' title='${lbl:b_outsourced_unit_business}' width='11' align='left' format='######' />
			<col name='trs_responsable' title='${lbl:b_responsible}' width='11' align='left' />
			<col name='trs_direccion' title='${lbl:b_address}' width='11' align='left' format='######' />
			<col name='trs_telefono_fijo' title='${lbl:b_fixed_phone}' width='11' align='left' />
			<col name='trs_telefono_movil' title='${lbl:b_mobile_phone}' width='11' align='left' />
			<col name='trs_email' title='${lbl:s_profile_email}' width='11' align='left' />
			<col name='trs_fecha_registro' title='${lbl:b_store_date}' width='11' align='left' format='dd-MM-yyyy' />
		</table>

	</pdf-report>
</config>