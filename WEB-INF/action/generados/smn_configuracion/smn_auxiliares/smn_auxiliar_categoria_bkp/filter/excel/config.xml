<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnauxiliarcategoria.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
	
	<excel recordset='query-smnauxiliarcategoria.sql' sheetname='${lbl:b_auxiliary}' filename='filter.xls'>
		<col id='aca_codigo' label='${lbl:b_code}' />
		<col id='aca_descripcion' label='${lbl:b_description}' />
		<col id='aca_clase_auxiliar' label='${lbl:b_auxiliary_class}' />
		<col id='aca_tipo_persona_combo' label='${lbl:b_kind_of_person}' />
		<col id='aca_fecha_registro' label='${lbl:b_store_date}' />
	</excel>

</config>