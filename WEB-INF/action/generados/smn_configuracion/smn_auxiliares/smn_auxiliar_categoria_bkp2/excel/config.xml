<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnauxiliarcategoria.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
	
	<excel recordset='query-smnauxiliarcategoria.sql' sheetname='${lbl:b_auxiliary}' filename='smn_auxiliar_categoria.xls'>
		<col id='aca_naturaleza_auxiliar' label='${lbl:b_nature_auxiliary}' />
		<col id='aca_tipo_persona_combo' label='${lbl:b_kind_of_person}' />
		<col id='aca_empresa' label='${lbl:b_entity}' />
		<col id='aca_sucursal' label='${lbl:b_subsidiary}' />
		<col id='aca_unidad_negocio' label='${lbl:b_business_unit}' />
		<col id='aca_area_servicio' label='${lbl:b_service_area}' />
		<col id='aca_unidad_servicio' label='${lbl:b_service_unit}' />
		<col id='aca_fecha_registro' label='${lbl:b_store_date}' />
	</excel>

</config>