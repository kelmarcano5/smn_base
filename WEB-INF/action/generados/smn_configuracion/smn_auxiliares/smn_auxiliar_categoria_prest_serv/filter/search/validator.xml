<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='aca_descripcion' type='varchar' required='false' label='${lbl:b_description}' maxlen='64'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='aca_descripcion' rule='contains' />
	<parameter id='aca_clase_auxiliar' type='integer' required='false' label='${lbl:b_auxiliary_class}' maxlen='10'/>
	<parameter id='aca_tipo_persona' type='varchar' required='false' label='${lbl:b_kind_of_person}' maxlen='2'/>
	<parameter id='aca_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date_grater_than}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date_grater_than} mayor o igual que] no puede ser mayor que [${lbl:b_store_date_less_than} menor o igual que].' date1='aca_fecha_registro_ini' date2='aca_fecha_registro_fin' id='aca_fecha_registro_ini' />
	<parameter id='aca_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date_less_than}' maxlen='10'/>
</validator>
