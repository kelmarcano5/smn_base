		<content-type>text/html</content-type>
		<print mode='table' recordset='query-smn_auxiliar_categoria_id.sql' tag='smn_auxiliar_categoria_id_rows'/><print mode='table' recordset='query-atr_comunidad.sql' tag='atr_comunidad_rows'/><print mode='table' recordset='query-atr_auxiliar_corporacion.sql' tag='atr_auxiliar_corporacion_rows'/><print mode='table' recordset='query-atr_auxiliar_unidad_negocio.sql' tag='atr_auxiliar_unidad_negocio_rows'/><print mode='table' recordset='query-atr_tipo_doc_oficial.sql' tag='atr_tipo_doc_oficial_rows'/><print mode='table' recordset='query-atr_direccion.sql' tag='atr_direccion_rows'/><print mode='table' recordset='query-atr_cuenta_bancaria.sql' tag='atr_cuenta_bancaria_rows'/><print mode='table' recordset='query-atr_contactos.sql' tag='atr_contactos_rows'/><print mode='table' recordset='query-atr_cond_pago.sql' tag='atr_cond_pago_rows'/><print mode='table' recordset='query-atr_condicion_financiera.sql' tag='atr_condicion_financiera_rows'/>
		<print mode="form" recordset="_request" /> 
	</output>

</config>
