<?xml version='1.0' encoding='ISO-8859-1'?>
<config>

	<summary>
		${lbl:b_intermediary_assistant}
	</summary>

	<log>false</log>
	
	<set-request-attribute id="progress" value="file:/WEB-INF/action/progress/progress.htm"/>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>true</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id='query-smn_auxiliar_categoria_id.sql' source='sql' scope='transaction'/><recordset id='query-atr_comunidad.sql' source='sql' scope='transaction'/><recordset id='query-atr_auxiliar_corporacion.sql' source='sql' scope='transaction'/><recordset id='query-atr_auxiliar_unidad_negocio.sql' source='sql' scope='transaction'/><recordset id='query-atr_tipo_doc_oficial.sql' source='sql' scope='transaction'/><recordset id='query-atr_direccion.sql' source='sql' scope='transaction'/><recordset id='query-atr_cuenta_bancaria.sql' source='sql' scope='transaction'/><recordset id='query-atr_contactos.sql' source='sql' scope='transaction'/><recordset id='query-atr_cond_pago.sql' source='sql' scope='transaction'/><recordset id='query-atr_condicion_financiera.sql' source='sql' scope='transaction'/>
	</transaction>

