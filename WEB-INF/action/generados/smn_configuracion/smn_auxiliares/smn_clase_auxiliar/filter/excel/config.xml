<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnclaseauxiliar.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
	
	<excel recordset='query-smnclaseauxiliar.sql' sheetname='${lbl:b_auxiliary_classes}' filename='filter.xls'>
		<col id='cla_codigo' label='${lbl:b_code}' />
		<col id='cla_nombre' label='${lbl:b_name}' />
		<col id='cla_modulo' label='${lbl:b_module}' />
		<col id='cla_naturaleza' label='${lbl:b_nature}' />
		<col id='cla_fecha_registro' label='${lbl:b_store_date}' />
	</excel>

</config>