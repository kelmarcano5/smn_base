<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnafiliaciones.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
	
	<excel recordset='query-smnafiliaciones.sql' sheetname='${lbl:b_affiliations}' filename='filter.xls'>
		<col id='afc_codigo' label='${lbl:b_code}' />
		<col id='afc_descripcion' label='${lbl:b_description}' />
		<col id='acf_auxiliar_persona_natural' label='${lbl:b_person_assistant}' />
		<col id='afc_modulo' label='${lbl:b_module}' />
		<col id='afc_es_titular_combo' label='${lbl:b_titular}' />
		<col id='afc_titular' label='${lbl:b_person_assistant}' />
		<col id='afc_parentezco' label='${lbl:b_relationship}' />
		<col id='afc_fecha_registro' label='${lbl:b_store_date}' />
	</excel>

</config>