<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_terceros_id_name' type='varchar' required='false' label='${lbl:b_outsourced_company}'/>
	<parameter id='smn_terceros_id' type='integer' required='false' label='${lbl:b_outsourced_company}' maxlen='10'/>
	<parameter id='trm_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='trm_codigo' rule='contains' />
	<parameter id='trm_nombre' type='varchar' required='false' label='${lbl:b_name}' maxlen='56'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='trm_nombre' rule='contains' />
	<parameter id='trm_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date_grater_than}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date_grater_than} mayor o igual que] no puede ser mayor que [${lbl:b_store_date_less_than} menor o igual que].' date1='trm_fecha_registro_ini' date2='trm_fecha_registro_fin' id='trm_fecha_registro_ini' />
	<parameter id='trm_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date_less_than}' maxlen='10'/>
</validator>
