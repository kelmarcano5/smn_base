<?xml version='1.0' encoding='ISO-8859-1'?>
<config>

	<summary>
		${lbl:b_auxiliary}
	</summary>

	<log>false</log>
	
	<set-request-attribute id="progress" value="file:/WEB-INF/action/progress/progress.htm"/>
	
	<clear-session id="detail-smn_auxiliar_tercero.sql"/>


	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>true</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id='query-aca_clase_auxiliar.sql' source='sql' scope='transaction'/>
		<recordset id='query-atr_tipo_doc_oficial.sql' source='sql' scope='transaction'/>
		<recordset id='query-atr_direccion.sql' source='sql' scope='transaction'/>
		<recordset id='query-atr_cuenta_bancaria.sql' source='sql' scope='transaction'/>
		<recordset id='query-atr_contactos.sql' source='sql' scope='transaction'/>
		<recordset id='query-atr_cond_pago.sql' source='sql' scope='transaction'/>
		<recordset id='query-atr_condicion_financiera.sql' source='sql' scope='transaction'/>

	</transaction>

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>template.htm</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/html</content-type>
		<print mode='table' recordset='query-aca_clase_auxiliar.sql' tag='aca_clase_auxiliar_rows'/>
		<print mode='table' recordset='query-atr_tipo_doc_oficial.sql' tag='atr_tipo_doc_oficial2_rows'/>
		<print mode='table' recordset='query-atr_direccion.sql' tag='atr_direccion2_rows'/>
		<print mode='table' recordset='query-atr_cuenta_bancaria.sql' tag='atr_cuenta_bancaria2_rows'/>
		<print mode='table' recordset='query-atr_contactos.sql' tag='atr_contactos2_rows'/>
		<print mode='table' recordset='query-atr_cond_pago.sql' tag='atr_cond_pago2_rows'/>
		<print mode='table' recordset='query-atr_condicion_financiera.sql' tag='atr_condicion_financiera2_rows'/>

		<print mode="form" recordset="_request" /> 
	</output>

</config>
