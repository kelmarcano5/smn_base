<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnmodeloscontratos.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>

	<pdf-title>${lbl:b_contract_models}</pdf-title>
	<pdf-logo url='/images/logo-dinamica.png' scale='100' />

	<pdf-report pageSize="letter" rotate="false">
		<table recordset='query-smnmodeloscontratos.sql' width='100' spacingBefore='40'>
			<col name='mct_modulo' title='${lbl:b_module}' width='16' align='left' format='######' />
			<col name='mct_archivo' title='${lbl:b_file}' width='16' align='left' />
			<col name='mct_contenido' title='${lbl:b_content}' width='16' align='left' />
			<col name='mct_descripcion' title='${lbl:b_description}' width='16' align='left' />
			<col name='mct_tamano' title='${lbl:b_size}' width='16' align='center' format='######' />
			<col name='mct_fecha_registro' title='${lbl:b_store_date}' width='16' align='center' format='dd-MM-yyyy' />
		</table>

	</pdf-report>
</config>