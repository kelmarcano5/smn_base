<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lbl:b_auxiliary_contact}</pdf-title>
	<pdf-logo url='/images/logo-dinamica.png' scale='100' />

	<pdf-report pageSize="letter" rotate="false">
		<record recordset='query.sql' title='${lbl:b_auxiliary_contact}' width='60' spacingBefore='40'>
			<col name='acn_telefono_fijo' title='${lbl:b_fixed_phone}' align='left' />
			<col name='acn_telefono_movil' title='${lbl:b_mobile_phone}' align='left' />
			<col name='acn_email' title='${lbl:s_profile_email}' align='left' />
			<col name='acn_facebook' title='${lbl:b_facebook}' align='left' />
			<col name='acn_linkedin' title='${lbl:b_linkedin}' align='left' />
			<col name='acn_twitter' title='${lbl:b_twitter}' align='left' />
			<col name='acn_swift' title='${lbl:b_swift_code}' align='left' />
			<col name='acn_fecha_registro' title='${lbl:b_store_date}' align='left' format='dd-MM-yyyy' />
		</record>

	</pdf-report>
</config>