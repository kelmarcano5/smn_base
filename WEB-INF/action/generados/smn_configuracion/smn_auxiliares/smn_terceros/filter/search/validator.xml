<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='ter_auxiliar_categoria' type='integer' required='false' label='${lbl:b_auxiliary_category}' maxlen='16'/>
	<parameter id='ter_descripcion_larga' type='varchar' required='false' label='${lbl:b_long_description}' maxlen='48'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='ter_descripcion_larga' rule='contains' />
	<parameter id='ter_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date_grater_than}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date_grater_than} mayor o igual que] no puede ser mayor que [${lbl:b_store_date_less_than} menor o igual que].' date1='ter_fecha_registro_ini' date2='ter_fecha_registro_fin' id='ter_fecha_registro_ini' />
	<parameter id='ter_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date_less_than}' maxlen='10'/>
</validator>
