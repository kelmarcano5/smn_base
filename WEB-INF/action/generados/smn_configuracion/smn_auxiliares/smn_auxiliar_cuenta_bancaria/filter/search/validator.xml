<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='acb_num_cuenta' type='varchar' required='false' label='${lbl:b_account_number}' maxlen='24'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='acb_num_cuenta' rule='contains' />
	<parameter id='acb_tipo_cta_banco' type='integer' required='false' label='${lbl:b_account_bank_type}' maxlen='1'/>
	<parameter id='acb_respons_cuenta' type='varchar' required='false' label='${lbl:b_responsible}' maxlen='96'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='acb_respons_cuenta' rule='contains' />
	<parameter id='acb_banco_pertenece' type='integer' required='false' label='${lbl:b_bank}' maxlen='3'/>
	<parameter id='acb_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date_grater_than}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date_grater_than} mayor o igual que] no puede ser mayor que [${lbl:b_store_date_less_than} menor o igual que].' date1='acb_fecha_registro_ini' date2='acb_fecha_registro_fin' id='acb_fecha_registro_ini' />
	<parameter id='acb_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date_less_than}' maxlen='10'/>
</validator>
