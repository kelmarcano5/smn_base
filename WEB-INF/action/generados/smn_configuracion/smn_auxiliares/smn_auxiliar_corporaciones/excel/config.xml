<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnauxiliarcorporaciones.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
	
	<excel recordset='query-smnauxiliarcorporaciones.sql' sheetname='${lbl:b_corporations}' filename='smn_auxiliar_corporaciones.xls'>
		<col id='tcp_codigo' label='${lbl:b_code}' />
		<col id='tcp_nombre' label='${lbl:b_name}' />
		<col id='tcp_auxiliar_unidad_negocio' label='${lbl:b_outsourced_unit_business}' />
		<col id='tcp_moneda' label='${lbl:b_coin}' />
		<col id='tcp_direccion' label='${lbl:b_address}' />
		<col id='tcp_fecha_registro' label='${lbl:b_store_date}' />
	</excel>

</config>