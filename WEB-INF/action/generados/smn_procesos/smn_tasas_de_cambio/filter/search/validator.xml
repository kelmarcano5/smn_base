<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_monedas_id_name' type='varchar' required='false' label='${lbl:b_coin}'/>
	<parameter id='smn_monedas_id' type='integer' required='false' label='${lbl:b_coin}' maxlen='10'/>
	<parameter id='tca_moneda_referencia' type='integer' required='false' label='${lbl:b_referenced_coin}' maxlen='10'/>
	<parameter id='tca_fecha_vigencia_ini' type='date' required='false' label='${lbl:b_validation_date_from}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_validation_date_from} mayor o igual que] no puede ser mayor que [${lbl:b_validation_date_until} menor o igual que].' date1='tca_fecha_vigencia_ini' date2='tca_fecha_vigencia_fin' id='tca_fecha_vigencia_ini' />
	<parameter id='tca_fecha_vigencia_fin' type='date' required='false' label='${lbl:b_validation_date_until}' maxlen='10'/>
	<parameter id='tca_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date_grater_than}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date_grater_than} mayor o igual que] no puede ser mayor que [${lbl:b_store_date_less_than} menor o igual que].' date1='tca_fecha_registro_ini' date2='tca_fecha_registro_fin' id='tca_fecha_registro_ini' />
	<parameter id='tca_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date_less_than}' maxlen='10'/>
</validator>
