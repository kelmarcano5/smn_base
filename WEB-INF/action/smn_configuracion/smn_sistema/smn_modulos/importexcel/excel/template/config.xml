<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnmodulos.sql" source="session" scope="transaction" />
		<recordset id='query-smnentidades.sql' source='sql' scope='transaction' />
	
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_modules}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_modules}">
				<col id='mod_empresa_combo' label='${lbl:b_entity}' />
		<col id='mod_codigo' label='${lbl:b_code}' />
		<col id='mod_nombre' label='${lbl:b_name}' />
		<col id='smn_app_rf_combo' label='${lbl:b_application}' />
		<col id='mod_ruta_anexado' label='${lbl:b_ruta_anexados}' />
		<col id='mod_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
		<sheet recordset='query-smnentidades.sql' sheetname=''>
		<col id='ent_descripcion_corta' label='${lbl:b_description}' />
	</sheet>


	</excel>

</config>
