<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="4" cellStep="0" name="${lbl:b_modules}"/>
	
	<excel 
		recordset='query.sql' 
		sheetname='${lbl:b_modules}' 
		filename='${lbl:b_modules}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='ent_descripcion_corta_combo' label='${lbl:b_entity}' />
		<col id='mod_codigo' label='${lbl:b_code}' />
		<col id='mod_nombre' label='${lbl:b_name}' />
		<col id='smn_app_rf_combo' label='${lbl:b_application}' />
		<col id='mod_ruta_anexado' label='${lbl:b_ruta_anexados}' />
		<col id='mod_idioma' label='${lbl:s_profile_lang}' />
		<col id='mod_usuario' label='${lbl:b_users}' />
		<col id='mod_fecha_registro' label='${lbl:b_store_date}' />
		<col id='mod_hora' label='${lbl:s_loginhist_col2}' />

	</excel>

</config>
