<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='mnt_app_id' type='integer' required='false' label='${lbl:b_application}' maxlen='10'/>
	<parameter id='mnt_menu_id' type='integer' required='false' label='${lbl:b_menu}' maxlen='10'/>
	<parameter id='mnt_title' type='varchar' required='false' label='${lbl:b_menu_title}' maxlen='100'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='mnt_title' rule='contains' />
	<parameter id='mnt_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date_grater_than}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date_grater_than} mayor o igual que] no puede ser mayor que [${lbl:b_store_date_less_than} menor o igual que].' date1='mnt_fecha_registro_ini' date2='mnt_fecha_registro_fin' id='mnt_fecha_registro_ini' />
	<parameter id='mnt_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date_less_than}' maxlen='10'/>
</validator>
