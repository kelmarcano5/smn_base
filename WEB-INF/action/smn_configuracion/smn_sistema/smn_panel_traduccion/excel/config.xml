<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnpaneltraduccion.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="2" cellStep="0" name="${lbl:b_panel_translation}"/>
	
	<excel 
		recordset='query-smnpaneltraduccion.sql' 
		sheetname='${lbl:b_panel_translation}' 
		filename='${lbl:b_panel_translation}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='ent_descripcion_corta_combo' label='${lbl:b_panel}' />
		<col id='pat_description' label='${lbl:b_description}' />
		<col id='pat_locale_combo' label='${lbl:b_idiom}' />
		<col id='pat_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
