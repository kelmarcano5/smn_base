<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnpaneltraduccion.sql" source="session" scope="transaction" />
		<recordset id='query-smnentidades.sql' source='sql' scope='transaction' />
	
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_panel_translation}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_panel_translation}">
				<col id='pat_panel_combo' label='${lbl:b_panel}' />
		<col id='pat_description' label='${lbl:b_description}' />
		<col id='pat_locale_combo' label='${lbl:b_idiom}' />
		<col id='pat_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
		<sheet recordset='query-smnentidades.sql' sheetname=''>
		<col id='ent_descripcion_corta' label='${lbl:b_description}' />
	</sheet>


	</excel>

</config>
