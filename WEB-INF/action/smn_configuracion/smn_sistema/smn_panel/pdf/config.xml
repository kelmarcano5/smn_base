<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnpanel.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lbl:b_panel}</pdf-title>
	<pdf-logo url='/images/logo-simone-pdf.png' scale='100' />
	<pdf-file-name>${lbl:b_panel}.pdf</pdf-file-name>

	<pdf-report 
		pageSize="letter" 
		rotate="false"
		footer="false" 
		header="true">
		<record recordset='query-smnpanel.sql' width='100' spacingBefore='40'>
			<col name='pan_service_description' title='${lbl:b_service_description}' width='14' align='left' />
			<col name='pan_service_path' title='${lbl:b_service_path}' width='14' align='left' />
			<col name='pan_icon_path' title='${lbl:b_icon_path}' width='14' align='left' />
			<col name='app_alias' title='${lbl:b_application}' width='14' align='left' />
			<col name='rolename' title='${lbl:b_role}' width='14' align='left' />
			<col name='pan_orden' title='${lbl:b_order}' width='14' align='left' format='######' />
			<col name='pan_fecha_registro' title='${lbl:b_store_date}' width='14' align='left' format='dd-MM-yyyy' />

		</record>

	</pdf-report>
</config>
