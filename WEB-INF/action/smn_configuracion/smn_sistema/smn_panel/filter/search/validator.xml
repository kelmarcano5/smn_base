<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='pan_service_description' type='varchar' required='false' label='${lbl:b_service_description}' maxlen='350'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='pan_service_description' rule='contains' />
	<parameter id='pan_app_id' type='integer' required='false' label='${lbl:b_application}' maxlen='10'/>
	<parameter id='pan_role_id' type='integer' required='false' label='${lbl:b_role}' maxlen='10'/>
	<parameter id='pan_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date_grater_than}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date_grater_than} mayor o igual que] no puede ser mayor que [${lbl:b_store_date_less_than} menor o igual que].' date1='pan_fecha_registro_ini' date2='pan_fecha_registro_fin' id='pan_fecha_registro_ini' />
	<parameter id='pan_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date_less_than}' maxlen='10'/>
</validator>
