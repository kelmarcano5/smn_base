<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="0" cellPosX="4" cellStep="0" name="${lbl:b_panel}"/>
	
	<excel 
		recordset='query.sql' 
		sheetname='${lbl:b_panel}' 
		filename='${lbl:b_panel}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='pan_service_description' label='${lbl:b_service_description}' />
		<col id='pan_service_path' label='${lbl:b_service_path}' />
		<col id='pan_icon_path' label='${lbl:b_icon_path}' />
		<col id='app_alias' label='${lbl:b_application}' />
		<col id='rolename' label='${lbl:b_role}' />
		<col id='pan_orden' label='${lbl:b_order}' />
		<col id='pan_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
