<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnentidadesfederales.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="0" cellPosX="3" cellStep="0" name="${lbl:b_federal_entities}"/>
	
	<excel 
		recordset='query-smnentidadesfederales.sql' 
		sheetname='${lbl:b_federal_entities}' 
		filename='${lbl:b_federal_entities}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='pai_nombre_largo' label='${lbl:b_country}' />
		<col id='efe_codigo' label='${lbl:b_code}' />
		<col id='efe_nombre_corto' label='${lbl:b_short_name}' />
		<col id='efe_nombre_largo' label='${lbl:b_long_name}' />
		<col id='efe_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
