<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="false">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnmunicipios-template.sql" source="sql" scope="transaction" />
		<recordset id='query-smnciudades.sql' source='sql' scope='transaction' />
	</transaction>

	<output>
		<classname>domain.repgen.smn_configuracion.smn_ubicacion_geografica.smn_municipios.ExcelReportOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_municipalities}.xls'
		header-background='true' >
		
	<sheet recordset="query-smnmunicipios-template.sql" sheetname="${lbl:b_municipalities}">
		
		<col id='mun_ciudades' label='${lbl:b_city}' />
		<col id='mun_codigo' label='${lbl:b_code}' />
		<col id='mun_nombre_corto' label='${lbl:b_short_name}' />
		<col id='mun_nombre_largo' label='${lbl:b_long_name}' />
		
		<col id='mun_ciudades_reference' label='${lbl:b_city}' />

	</sheet>
		<sheet recordset='query-smnciudades.sql' sheetname='${lbl:b_city}'>
		<col id='ciu_nombre_corto' label='${lbl:b_name}' />
	</sheet>


	</excel>

</config>
