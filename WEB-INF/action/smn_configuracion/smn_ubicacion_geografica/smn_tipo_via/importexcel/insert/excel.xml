<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='tiv_codigo' type='varchar' required='true' maxlen='16'/>
	<parameter id='tiv_descripcion' type='varchar' required='true' maxlen='48'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_code_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='tiv_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_desc_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='tiv_descripcion'/>


</validator>
