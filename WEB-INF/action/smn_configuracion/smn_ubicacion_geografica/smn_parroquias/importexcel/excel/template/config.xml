<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="false">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnparroquias-template.sql" source="sql" scope="transaction" />
		<recordset id='query-smnmunicipios.sql' source='sql' scope='transaction' />
	</transaction>

	<output>
		<classname>domain.repgen.smn_configuracion.smn_ubicacion_geografica.smn_parroquias.ExcelReportOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_parishes}.xls'
		header-background='true' >
		
	<sheet recordset="query-smnparroquias-template.sql" sheetname="${lbl:b_parishes}">
		
		<col id='prr_municipios' label='${lbl:b_municipality}' />
		<col id='prr_codigo' label='${lbl:b_code}' />
		<col id='prr_nombre_corto' label='${lbl:b_short_name}' />
		<col id='prr_nombre_largo' label='${lbl:b_long_name}' />
		
		<col id='prr_municipios_reference' label='${lbl:b_municipality}' />

	</sheet>
		<sheet recordset='query-smnmunicipios.sql' sheetname='${lbl:b_municipality}'>
		<col id='mun_nombre_corto' label='${lbl:b_name}' />
	</sheet>


	</excel>

</config>
