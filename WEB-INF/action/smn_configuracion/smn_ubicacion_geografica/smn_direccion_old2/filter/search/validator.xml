<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='dir_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='dir_codigo' rule='contains' />
	<parameter id='dir_descripcion' type='varchar' required='false' label='${lbl:b_description}' maxlen='48'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='dir_descripcion' rule='contains' />
	<parameter id='dir_pais' type='integer' required='false' label='${lbl:b_country}' maxlen='10'/>
	<parameter id='dir_entidad_federal' type='integer' required='false' label='${lbl:b_federal_entity}' maxlen='10'/>
	<parameter id='dir_ciudad' type='integer' required='false' label='${lbl:b_city}' maxlen='10'/>
	<parameter id='dir_municipio' type='integer' required='false' label='${lbl:b_municipality}' maxlen='10'/>
	<parameter id='dir_parroquia' type='integer' required='false' label='${lbl:b_parish}' maxlen='10'/>
	<parameter id='dir_zona_postal' type='integer' required='false' label='${lbl:b_postal_zone}' maxlen='8'/>
	<parameter id='dir_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date_grater_than}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date_grater_than} mayor o igual que] no puede ser mayor que [${lbl:b_store_date_less_than} menor o igual que].' date1='dir_fecha_registro_ini' date2='dir_fecha_registro_fin' id='dir_fecha_registro_ini' />
	<parameter id='dir_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date_less_than}' maxlen='10'/>
</validator>
