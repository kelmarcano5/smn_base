<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="false">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smndireccion-template.sql" source="sql" scope="transaction" />
		<recordset id='query-smnciudades.sql' source='sql' scope='transaction' />
		<recordset id='query-smnedificacion.sql' source='sql' scope='transaction' />
		<recordset id='query-smnentidadesfederales.sql' source='sql' scope='transaction' />
		<recordset id='query-smnmunicipios.sql' source='sql' scope='transaction' />
		<recordset id='query-smnpaises.sql' source='sql' scope='transaction' />
		<recordset id='query-smnparroquias.sql' source='sql' scope='transaction' />
		<recordset id='query-smntipolocalidad.sql' source='sql' scope='transaction' />
		<recordset id='query-smntipovia.sql' source='sql' scope='transaction' />
		<recordset id='query-smnzonaspostales.sql' source='sql' scope='transaction' />
	
	</transaction>

	<output>
		<classname>domain.repgen.smn_configuracion.smn_ubicacion_geografica.smn_direccion.ExcelReportOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_addresses}.xls'
		header-background='true' >
		
	<sheet recordset="query-smndireccion-template.sql" sheetname="${lbl:b_addresses}">
		
		<col id='dir_codigo' label='${lbl:b_code}' />
		<col id='dir_descripcion' label='${lbl:b_description}' />
		<col id='dir_pais_reference' label='${lbl:b_country}' />
		<col id='dir_entidad_federal_reference' label='${lbl:b_federal_entity}' />
		<col id='dir_ciudad_reference' label='${lbl:b_city}' />
		<col id='dir_municipio_reference' label='${lbl:b_municipality}' />
		<col id='dir_parroquia_reference' label='${lbl:b_parish}' />
		<col id='dir_zona_postal_reference' label='${lbl:b_postal_zone}' />
		<col id='dir_tipo_via_reference' label='${lbl:b_type_of_route}' />
		<col id='dir_tipo_via_nom' label='${lbl:b_name}' />
		<col id='dir_tipo_via_num' label='${lbl:b_number}' />
		<col id='dir_edificacion_reference' label='${lbl:b_edification}' />
		<col id='dir_edificacion_nom' label='${lbl:b_name}' />
		<col id='dir_edificacion_num' label='${lbl:b_number}' />
		<col id='dir_tipo_localidad_reference' label='${lbl:b_location_type}' />
		<col id='dir_tipo_localidad_nom' label='${lbl:b_name}' />
		<col id='dir_apartado_postal' label='${lbl:b_postal_mail}' />
		<col id='dir_categoria' label='${lbl:b_category}' />
		<col id='dir_mapa_longitud' label='${lbl:b_longitude}' />
		<col id='dir_mapa_latitud' label='${lbl:b_latitude}' />

	</sheet>
		<sheet recordset='query-smnciudades.sql' sheetname='${lbl:b_city}'>
		<col id='ciu_nombre_corto' label='${lbl:b_name}' />
	</sheet>

	<sheet recordset='query-smnedificacion.sql' sheetname='${lbl:b_edification}'>
		<col id='edf_descripcion' label='${lbl:b_description}' />
	</sheet>

	<sheet recordset='query-smnentidadesfederales.sql' sheetname='${lbl:b_federal_entity}'>
		<col id='efe_nombre_corto' label='${lbl:b_name}' />
	</sheet>

	<sheet recordset='query-smnmunicipios.sql' sheetname='${lbl:b_municipality}'>
		<col id='mun_nombre_corto' label='${lbl:b_name}' />
	</sheet>

	<sheet recordset='query-smnpaises.sql' sheetname='${lbl:b_country}'>
		<col id='pai_nombre_corto' label='${lbl:b_name}' />
	</sheet>

	<sheet recordset='query-smnparroquias.sql' sheetname='${lbl:b_parish}'>
		<col id='prr_nombre_corto' label='${lbl:b_name}' />
	</sheet>

	<sheet recordset='query-smntipolocalidad.sql' sheetname='${lbl:b_location_type}'>
		<col id='til_descripcion' label='${lbl:b_description}' />
	</sheet>

	<sheet recordset='query-smntipovia.sql' sheetname='${lbl:b_type_of_route}'>
		<col id='tiv_descripcion' label='${lbl:b_description}' />
	</sheet>

	<sheet recordset='query-smnzonaspostales.sql' sheetname='${lbl:b_postal_zone}'>
		<col id='zpo_codigo' label='${lbl:b_code}' />
	</sheet>


	</excel>

</config>
