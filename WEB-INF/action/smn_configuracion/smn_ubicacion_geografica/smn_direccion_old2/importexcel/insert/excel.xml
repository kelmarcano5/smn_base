	<parameter id='dir_mapa_longitud' type='double' required='false' maxlen='10'/>
	<parameter id='dir_mapa_latitud' type='double' required='false' maxlen='10'/>

	<parameter id='dir_pais_ref' type='integer' required='false' maxlen='10'/>
