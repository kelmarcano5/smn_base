	<parameter id='dir_zona_postal_desc' type='varchar' required='true' maxlen='8'/>
	<parameter id='dir_tipo_via_desc' type='varchar' required='true' maxlen='48'/>
	<parameter id='dir_tipo_via_nom' type='varchar' required='true' maxlen='48'/>
