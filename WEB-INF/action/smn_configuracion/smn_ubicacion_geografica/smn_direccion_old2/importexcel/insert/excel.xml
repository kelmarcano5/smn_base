		parameter='dir_tipo_via_ref' 
		sql='dir_tipo_via.sql' 
		id='dir_tipo_via_ref'/>

	<custom-validator 
		classname='dinamica.validators.GetColValueValidator' 
		on-error-label='${lbl:b_field_does_not_registered}: dir_edificacion' 
		parameter='dir_edificacion_ref' 
		sql='dir_edificacion.sql' 
		id='dir_edificacion_ref'/>

	<custom-validator 
		classname='dinamica.validators.GetColValueValidator' 
		on-error-label='${lbl:b_field_does_not_registered}: dir_tipo_localidad' 
		parameter='dir_tipo_localidad_ref' 
		sql='dir_tipo_localidad.sql' 
		id='dir_tipo_localidad_ref'/>

