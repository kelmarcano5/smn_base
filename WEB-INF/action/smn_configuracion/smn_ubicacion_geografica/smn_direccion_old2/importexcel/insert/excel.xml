	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_code_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='dir_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_desc_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='dir_descripcion'/>


	<custom-validator 
		classname='dinamica.validators.GetColValueValidator' 
		on-error-label='${lbl:b_field_does_not_registered}: dir_pais' 
		parameter='dir_pais_ref' 
		sql='dir_pais.sql' 
		id='dir_pais_ref'/>

	<custom-validator 
		classname='dinamica.validators.GetColValueValidator' 
		on-error-label='${lbl:b_field_does_not_registered}: dir_entidad_federal' 
		parameter='dir_entidad_federal_ref' 
