<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='trg_codigo' type='varchar' required='true' maxlen='16'/>
	<parameter id='trg_descripcion' type='varchar' required='true' maxlen='48'/>
	<parameter id='trg_modulo_origen_desc' type='varchar' required='true' maxlen='48'/>

	<parameter id='trg_modulo_origen_ref' type='integer' required='false' maxlen='10'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='trg_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='trg_descripcion'/>

	<custom-validator 
		classname='dinamica.validators.GetColValueValidator' 
		on-error-label='${lbl:b_field_does_not_registered}: trg_modulo_origen' 
		parameter='trg_modulo_origen_ref' 
		sql='trg_modulo_origen.sql' 
		id='trg_modulo_origen_ref'/>

</validator>
