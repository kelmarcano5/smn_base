<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='acc_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='acc_codigo' rule='like' />
	<parameter id='acc_descripcion' type='varchar' required='false' label='${lbl:b_description}' maxlen='100'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='acc_descripcion' rule='like' />
	<parameter id='acc_tipo' type='varchar' required='false' label='${lbl:b_kind}' maxlen='2'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='acc_tipo' rule='like' />
	<parameter id='acc_fecha_registro' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
