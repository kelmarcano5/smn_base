<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnnivelaprobacion.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_nivel_aprobacion}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_nivel_aprobacion}">
				<col id='smn_configuracion_estructura_id_combo' label='${lbl:b_configuracion_estructura}' />
		<col id='nap_codigo' label='${lbl:b_code}' />
		<col id='nap_descripcion' label='${lbl:b_description}' />
		<col id='nap_rol_combo' label='${lbl:b_role}' />
		<col id='nap_secuencia_aprobacion' label='${lbl:b_approval_sequence}' />
		<col id='nap_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
	
	</excel>

</config>
