<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='smn_configuracion_estructura_id' type='integer' required='true' maxlen='10'/>
	<parameter id='nap_codigo' type='varchar' required='true' maxlen='10'/>
	<parameter id='nap_descripcion' type='varchar' required='true' maxlen='100'/>
	<parameter id='nap_rol' type='varchar' required='true' maxlen='2'/>
	<parameter id='nap_secuencia_aprobacion' type='integer' required='true' maxlen='10'/>


	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='nap_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='nap_descripcion'/>


</validator>
