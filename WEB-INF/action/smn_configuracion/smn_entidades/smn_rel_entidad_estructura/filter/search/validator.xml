<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_entidades_id_name' type='varchar' required='false' label='${lbl:b_entities}'/>
	<parameter id='smn_entidades_id' type='integer' required='false' label='${lbl:b_entities}' maxlen='10'/>
	<parameter id='smn_configuracion_estructura_id_name' type='varchar' required='false' label='${lbl:b_structure_organizational}'/>
	<parameter id='smn_configuracion_estructura_id' type='integer' required='false' label='${lbl:b_structure_organizational}' maxlen='10'/>
	<parameter id='rec_status' type='varchar' required='false' label='${lbl:b_status}' maxlen='1'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='rec_status' rule='like' />
	<parameter id='rec_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='rec_fecha_registro_ini' date2='rec_fecha_registro_fin' id='rec_fecha_registro_ini' />
	<parameter id='rec_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
