<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnrelentidadestructura.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lbl:b_structure_entities}</pdf-title>
	<pdf-logo url='/images/logo-simone-pdf.png' scale='100' />
	<pdf-file-name>${lbl:b_structure_entities}.pdf</pdf-file-name>

	<pdf-report 
		pageSize="letter" 
		rotate="false"
		footer="false" 
		header="true">
		<record recordset='query-smnrelentidadestructura.sql' width='100' spacingBefore='40'>
			<col name='smn_entidades_id' title='${lbl:b_entities}' width='25' align='left' format='######' />
			<col name='smn_configuracion_estructura_id' title='${lbl:b_structure_organizational}' width='25' align='left' format='######' />
			<col name='rec_status' title='${lbl:b_status}' width='25' align='left' />
			<col name='rec_fecha_registro' title='${lbl:b_store_date}' width='25' align='left' format='dd-MM-yyyy' />

		</record>

	</pdf-report>
</config>
