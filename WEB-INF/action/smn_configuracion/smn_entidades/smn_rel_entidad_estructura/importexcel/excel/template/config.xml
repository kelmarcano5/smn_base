<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnrelentidadestructura.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_structure_entities}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_structure_entities}">
				<col id='smn_entidades_id' label='${lbl:b_entities}' />
		<col id='smn_configuracion_estructura_id' label='${lbl:b_structure_organizational}' />
		<col id='rec_status' label='${lbl:b_status}' />
		<col id='rec_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
	
	</excel>

</config>
