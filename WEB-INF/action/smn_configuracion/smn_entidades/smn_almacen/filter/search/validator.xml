<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='alm_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='alm_codigo' rule='contains' />
	<parameter id='alm_nombre' type='varchar' required='false' label='${lbl:b_name}' maxlen='48'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='alm_nombre' rule='contains' />
	<parameter id='alm_empresa' type='integer' required='false' label='${lbl:b_entity}' maxlen='10'/>
	<parameter id='alm_unidad_negocio' type='integer' required='false' label='${lbl:b_business_unit}' maxlen='10'/>
	<parameter id='alm_sucursal' type='integer' required='false' label='${lbl:b_subsidiary}' maxlen='10'/>
	<parameter id='alm_area_servicio' type='integer' required='false' label='${lbl:b_service_area}' maxlen='10'/>
	<parameter id='alm_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date_grater_than}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date_grater_than} mayor o igual que] no puede ser mayor que [${lbl:b_store_date_less_than} menor o igual que].' date1='alm_fecha_registro_ini' date2='alm_fecha_registro_fin' id='alm_fecha_registro_ini' />
	<parameter id='alm_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date_less_than}' maxlen='10'/>
</validator>
