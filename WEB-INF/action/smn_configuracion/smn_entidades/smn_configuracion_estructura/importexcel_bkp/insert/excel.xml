<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='coe_codigo' type='varchar' required='true' maxlen='16'/>
	<parameter id='coe_nombre' type='varchar' required='true' maxlen='48'/>
	<parameter id='coe_niveles' type='integer' required='true' maxlen='10'/>
	<parameter id='coe_mascara' type='varchar' required='true' maxlen='48'/>
	<parameter id='coe_separador' type='varchar' required='true' maxlen='1'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='coe_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarNombre.sql'
			id='coe_nombre'/>

</validator>
