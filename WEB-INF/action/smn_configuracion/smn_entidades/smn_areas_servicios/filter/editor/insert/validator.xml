<?xml version='1.0' encoding='ISO-8859-1'?>
<validator onerror='/action/error/validation/ajax'>
	<parameter id='smn_tipos_areas_id_name' type='varchar' required='true' label='${lbl:b_types_area}'/>
	<parameter id='smn_tipos_areas_id' type='integer' required='false' label='${lbl:b_types_area}' maxlen='10'/>
	<parameter id='ase_codigo' type='varchar' required='true' label='${lbl:b_code}' maxlen='16'/>
	<parameter id='ase_descripcion' type='varchar' required='true' label='${lbl:b_description}' maxlen='48'/>
	<parameter id='ase_empresa' type='integer' required='true' label='${lbl:b_entity}' maxlen='10'/>
	<parameter id='ase_sucursal' type='integer' required='false' label='${lbl:b_subsidiary}' maxlen='10'/>
	<parameter id='ase_unidad_negocio' type='integer' required='false' label='${lbl:b_business_unit}' maxlen='10'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='ase_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='ase_descripcion'/>


</validator>
