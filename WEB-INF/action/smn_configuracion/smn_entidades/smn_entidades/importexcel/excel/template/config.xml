<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="false">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnentidades-template.sql" source="sql" scope="transaction" />
		<recordset id='query-smncomunidades.sql' source='sql' scope='transaction' />
		<recordset id='query-smncorporaciones.sql' source='sql' scope='transaction' />
		<recordset id='query-smndireccion.sql' source='sql' scope='transaction' />
		<recordset id='query-smnmonedas.sql' source='sql' scope='transaction' />
		<recordset id='query-smnsectoreseconomicos.sql' source='sql' scope='transaction' />
		<recordset id='query-smnunidadesnegocios.sql' source='sql' scope='transaction' />
		<recordset id='query-manejasucursal.sql' source='sql' scope='transaction' />
	</transaction>

	<output>
		<classname>domain.repgen.smn_configuracion.smn_entidades.ExcelReportOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_entities}.xls' 
		header-background='true' >
		
	<sheet recordset="query-smnentidades-template.sql" sheetname="${lbl:b_entities}">
		
		<col id='ent_codigo' label='${lbl:b_code}' />
		<col id='ent_comunidad' label='${lbl:b_community}' />
		<col id='ent_corporacion' label='${lbl:b_corporation}' />
		<col id='ent_unidad_negocio' label='${lbl:b_business_unit}' />
		<col id='ent_maneja_sucursal_combo' label='${lbl:b_has_subsidiary}' />
		<col id='ent_moneda' label='${lbl:b_currency}' />
		<col id='ent_sector_economico' label='${lbl:b_economic_sector}' />
		<col id='ent_descripcion_corta' label='${lbl:b_short_description}' />
		<col id='ent_descripcion_larga' label='${lbl:b_long_description}' />
		<col id='ent_nro_id_fiscal' label='${lbl:b_tax_identification_number}' />
		<col id='ent_direccion' label='${lbl:b_address}' />
		<col id='ent_contacto' label='${lbl:b_contact_person}' />
		<col id='ent_telefono_fijo' label='${lbl:b_fixed_phone}' />
		<col id='ent_telefono_movil' label='${lbl:b_mobile_phone}' />
		<col id='ent_email' label='${lbl:s_profile_email}' />
		<col id='ent_act_economica' label='${lbl:b_economic_activity}' />
		
		<col id='ent_comunidad_reference' label='${lbl:b_community}' />
		<col id='ent_corporacion_reference' label='${lbl:b_corporation}' />
		<col id='ent_unidad_negocio_reference' label='${lbl:b_business_unit}' />
		<col id='ent_maneja_sucursal_combo_reference' label='${lbl:b_has_subsidiary}' />
		<col id='ent_moneda_reference' label='${lbl:b_currency}' />
		<col id='ent_sector_economico_reference' label='${lbl:b_economic_sector}' />
		<col id='ent_direccion_reference' label='${lbl:b_address}' />
		
	</sheet>
	
	<sheet recordset='query-manejasucursal.sql' sheetname='${lbl:b_has_subsidiary}'>
		<col id='maneja_sucursal' label='${lbl:b_name}' />
	</sheet>
	
	<sheet recordset='query-smncomunidades.sql' sheetname='${lbl:b_community}'>
		<col id='cmn_nombre' label='${lbl:b_name}' />
	</sheet>

	<sheet recordset='query-smncorporaciones.sql' sheetname='${lbl:b_corporation}'>
		<col id='crp_nombre' label='${lbl:b_name}' />
	</sheet>

	<sheet recordset='query-smndireccion.sql' sheetname='${lbl:b_address}'>
		<col id='dir_descripcion' label='${lbl:b_description}' />
	</sheet>

	<sheet recordset='query-smnmonedas.sql' sheetname='${lbl:b_currency}'>
		<col id='mon_nombre' label='${lbl:b_name}' />
	</sheet>

	<sheet recordset='query-smnsectoreseconomicos.sql' sheetname='${lbl:b_economic_sector}'>
		<col id='sec_descripcion' label='${lbl:b_description}' />
	</sheet>

	<sheet recordset='query-smnunidadesnegocios.sql' sheetname='${lbl:b_business_unit}'>
		<col id='uen_nombre' label='${lbl:b_name}' />
	</sheet>

	</excel>

</config>
