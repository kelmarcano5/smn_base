<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="8" cellStep="0" name="${lbl:b_entities}"/>
	
	<excel 
		recordset='query.sql' 
		sheetname='${lbl:b_entities}' 
		filename='${lbl:b_entities}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='ent_codigo' label='${lbl:b_code}' />
		<col id='cmn_nombre' label='${lbl:b_community}' />
		<col id='crp_nombre' label='${lbl:b_corporation}' />
		<col id='uen_nombre' label='${lbl:b_business_unit}' />
		<col id='ent_maneja_sucursal_combo' label='${lbl:b_has_subsidiary}' />
		<col id='mon_nombre' label='${lbl:b_currency}' />
		<col id='sec_descripcion' label='${lbl:b_economic_sector}' />
		<col id='ent_descripcion_corta' label='${lbl:b_short_description}' />
		<col id='ent_descripcion_larga' label='${lbl:b_long_description}' />
		<col id='ent_nro_id_fiscal' label='${lbl:b_tax_identification_number}' />
		<col id='dir_descripcion' label='${lbl:b_address}' />
		<col id='ent_contacto' label='${lbl:b_contact_person}' />
		<col id='ent_telefono_fijo' label='${lbl:b_fixed_phone}' />
		<col id='ent_telefono_movil' label='${lbl:b_mobile_phone}' />
		<col id='ent_email' label='${lbl:s_profile_email}' />
		<col id='ent_act_economica' label='${lbl:b_economic_activity}' />
		<col id='ent_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
