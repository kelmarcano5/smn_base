<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smngrupoprestadorserviciofrecuencia.sql" source="session" scope="transaction" />
		<recordset id="filter.params" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lbl:b_groups_providers_services_frecuency}</pdf-title>
	<pdf-logo url='/images/logo-simone-pdf.png' scale='100' align="left" />
	<pdf-file-name>${lbl:b_groups_providers_services_frecuency}.pdf</pdf-file-name>

	<pdf-report pageSize="letter" rotate="false" footer="true" header="true">
		<record recordset='filter.params' title='${lbl:b_filter_find}' noFilterMsg='-- ${lbl:b_all_message} --' width='55' spacingBefore='15'>
			<col name='smn_grupos_prestadores_id_name' title='${lbl:b_groups_of_providers}'  align='left' />
			<col name='smn_prestadores_servicios_id_name' title='${lbl:b_service_provider}'  align='left' />
			<col name='smn_servicios_id_name' title='${lbl:b_service}'  align='left' />
			<col name='gps_fecha_registro_ini' title='${lbl:b_store_date}'  format='######' align='left' />
			<col name='gps_fecha_registro_fin' title='${lbl:b_store_date}'  format='######' align='left' />

		</record>
		<record recordset='query-smngrupoprestadorserviciofrecuencia.sql' width='100' spacingBefore='10'>
				<col name='smn_grupos_prestadores_id' title='${lbl:b_groups_of_providers}' align='left' />
			<col name='smn_prestadores_servicios_id' title='${lbl:b_service_provider}' align='left' />
			<col name='smn_servicios_id' title='${lbl:b_service}' align='left' />
			<col name='gps_frecuencia' title='${lbl:b_frecuency}' width='20' align='left' format='#,###,##0.00' />
				<col name='gps_fecha_registro' title='${lbl:b_store_date}' width='7' align='left' format='dd-MM-yyyy' />
		</record>

	</pdf-report>
</config>
