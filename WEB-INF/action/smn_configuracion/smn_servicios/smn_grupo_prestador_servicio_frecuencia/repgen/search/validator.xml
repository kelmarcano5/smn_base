<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='repgen.filter' onerror='/action/error/validation/ajax'>

	<parameter id='fdesde' type='date' required='false' label='Fecha desde'/>
	<parameter id='fhasta' type='date' required='false' label='Fecha hasta'/>
	
	<parameter id='smn_grupos_prestadores_id_name' type='varchar' required='false' label='${lbl:b_groups_of_providers}'/>
	<parameter id='smn_grupos_prestadores_id' type='integer' required='false' label='${lbl:b_groups_of_providers}' maxlen='10'/>
	<parameter id='smn_prestadores_servicios_id_name' type='varchar' required='false' label='${lbl:b_service_provider}'/>
	<parameter id='smn_prestadores_servicios_id' type='integer' required='false' label='${lbl:b_service_provider}' maxlen='10'/>
	<parameter id='smn_servicios_id_name' type='varchar' required='false' label='${lbl:b_service}'/>
	<parameter id='smn_servicios_id' type='integer' required='false' label='${lbl:b_service}' maxlen='10'/>
	<parameter id='gps_frecuencia' type='double' required='false' label='${lbl:b_frecuency}' maxlen='10'/>

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='smn_grupos_prestadores_id_name'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='smn_prestadores_servicios_id_name'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='smn_servicios_id_name'
		rule='contains' />



	<custom-validator
			classname='dinamica.validators.ArrayNotEmptyValidator'
			on-error-label='Seleccione al menos un campo para la generación del reporte.'
			parameter-name='field_id' 
			id='field_id' />

	<custom-validator 
			classname='dinamica.validators.ArrayToRecordset'
			paramName='field_id' 
			recordsetId='fields' 
			colName='field_id' 
			colType='integer' />
				
</validator>
