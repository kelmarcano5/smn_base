<?xml version='1.0' encoding='ISO-8859-1'?>
<config>

	<summary>
		${lbl:b_service}
	</summary>

	<log>false</log>
	
	<set-request-attribute id="progress" value="file:/WEB-INF/action/progress/progress.htm"/>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>true</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id='query-smn_tipo_servicio_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_unidad_medida_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_moneda_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_descuento_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_clase_auxiliar_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_documento_general_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_grupo_titulo_impresion_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_baremos_id.sql' source='sql' scope='transaction'/>
	</transaction>

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>template.htm</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/html</content-type>
		<print mode='table' recordset='query-smn_tipo_servicio_rf.sql' tag='smn_tipo_servicio_rf_rows'/>
		<print mode='table' recordset='query-smn_unidad_medida_rf.sql' tag='smn_unidad_medida_rf_rows'/>
		<print mode='table' recordset='query-smn_moneda_rf.sql' tag='smn_moneda_rf_rows'/>
		<print mode='table' recordset='query-smn_descuento_rf.sql' tag='smn_descuento_rf_rows'/>
		<print mode='table' recordset='query-smn_clase_auxiliar_rf.sql' tag='smn_clase_auxiliar_rf_rows'/>
		<print mode='table' recordset='query-smn_documento_general_rf.sql' tag='smn_documento_general_rf_rows'/>
		<print mode='table' recordset='query-smn_grupo_titulo_impresion_rf.sql' tag='smn_grupo_titulo_impresion_rf_rows'/>
		<print mode='table' recordset='query-smn_baremos_id.sql' tag='smn_baremos_id_rows'/>
		<print mode="form" recordset="_request" /> 
	</output>

</config>
