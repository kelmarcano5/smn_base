<?xml version='1.0' encoding='ISO-8859-1'?>
<config>

	<summary>
		${lbl:b_baremos_detail}
	</summary>

	<log>false</log>
	
	<set-request-attribute id="progress" value="file:/WEB-INF/action/progress/progress.htm"/>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>true</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id='query-smn_baremos_id.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_servicios_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_clase_auxiliar_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_item_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_almacen_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_moneda_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_descuento_rf.sql' source='sql' scope='transaction'/>
		<recordset id="smn_detalle_id" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>template.htm</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/html</content-type>
		<print mode='table' recordset='query-smn_baremos_id.sql' tag='smn_baremos_id_rows'/><print mode='table' recordset='query-smn_servicios_rf.sql' tag='smn_servicios_rf_rows'/><print mode='table' recordset='query-smn_clase_auxiliar_rf.sql' tag='smn_clase_auxiliar_rf_rows'/><print mode='table' recordset='query-smn_item_rf.sql' tag='smn_item_rf_rows'/><print mode='table' recordset='query-smn_almacen_rf.sql' tag='smn_almacen_rf_rows'/><print mode='table' recordset='query-smn_moneda_rf.sql' tag='smn_moneda_rf_rows'/><print mode='table' recordset='query-smn_descuento_rf.sql' tag='smn_descuento_rf_rows'/>
		<print mode="form" recordset="smn_detalle_id"/>
		<print mode="form" recordset="_request" /> 
	</output>

</config>
