<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_servicios_id' type='integer' required='false' label='${lbl:b_service}' maxlen='10'/>
	<parameter id='smn_servicios_id2' type='varchar' required='false' label='${lbl:b_code_service}' maxlen='100'/>
		<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='smn_servicios_id2' rule='contains' />
	<parameter id='smn_servicios_id3' type='varchar' required='false' label='${lbl:b_description_service}' maxlen='100'/>
		<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='smn_servicios_id3' rule='contains' />
	<parameter id='smn_areas_servicios_rf' type='integer' required='false' label='${lbl:b_service_area}' maxlen='10'/>
	<parameter id='smn_unidades_servicios_rf' type='integer' required='false' label='${lbl:b_service_unit}' maxlen='10'/>
	<parameter id='smn_centro_costo_rf' type='integer' required='false' label='${lbl:b_cost_centers}' maxlen='10'/>
	<parameter id='rsa_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='rsa_fecha_registro_ini' date2='rsa_fecha_registro_fin' id='rsa_fecha_registro_ini' />
	<parameter id='rsa_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
