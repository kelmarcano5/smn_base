<?xml version='1.0' encoding='ISO-8859-1'?>
<config>

	<summary>
		${lbl:b_rel_services_area_unit}
	</summary>

	<log>false</log>
	
	<set-request-attribute id="progress" value="file:/WEB-INF/action/progress/progress.htm"/>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>true</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
			<recordset id='query-smn_servicios_id.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_areas_servicios_rf.sql' source='sql' scope='transaction'/><recordset id='query-smn_unidades_servicios_rf.sql' source='sql' scope='transaction'/><recordset id='query-smn_centro_costo_rf.sql' source='sql' scope='transaction'/>
		<recordset id="smn_detalle_id" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>template.htm</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/html</content-type>
			<print mode='table' recordset='query-smn_servicios_id.sql' tag='smn_servicios_id_rows'/>
		<print mode='table' recordset='query-smn_areas_servicios_rf.sql' tag='smn_areas_servicios_rf_rows'/><print mode='table' recordset='query-smn_unidades_servicios_rf.sql' tag='smn_unidades_servicios_rf_rows'/><print mode='table' recordset='query-smn_centro_costo_rf.sql' tag='smn_centro_costo_rf_rows'/>
		<print mode="form" recordset="smn_detalle_id"/>
	</output>

</config>
