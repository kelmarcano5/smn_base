<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnrelservicioareaunidad.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="2" cellStep="0" name="${lbl:b_rel_services_area_unit}"/>
	
	<excel 
		recordset='query-smnrelservicioareaunidad.sql' 
		sheetname='${lbl:b_rel_services_area_unit}' 
		filename='${lbl:b_rel_services_area_unit}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='svc_codigo_pl0' label='${lbl:b_service}' />
		<col id='smn_areas_servicios_rf' label='${lbl:b_service_area}' />
		<col id='smn_unidades_servicios_rf' label='${lbl:b_service_unit}' />
		<col id='smn_centro_costo_rf' label='${lbl:b_cost_centers}' />
		<col id='rsa_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
