	<parameter id='gpp_descripcion' type='varchar' required='true' label='${lbl:b_description}' maxlen='56'/>
	<parameter id='gpp_area_servicio' type='integer' required='false' label='${lbl:b_service_area}' maxlen='10'/>
	<parameter id='gpp_unidad_servicio' type='integer' required='false' label='${lbl:b_service_unit}' maxlen='10'/>
