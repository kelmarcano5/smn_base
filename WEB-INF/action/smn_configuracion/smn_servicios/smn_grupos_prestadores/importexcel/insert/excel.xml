<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='gpp_codigo' type='varchar' required='true' maxlen='16'/>
	<parameter id='gpp_descripcion' type='varchar' required='true' maxlen='56'/>
	<parameter id='gpp_area_servicio_desc' type='varchar' required='true' maxlen='48'/>
	<parameter id='gpp_unidad_servicio_desc' type='varchar' required='true' maxlen='48'/>
	<parameter id='gpp_orden_atencion_desc' type='varchar' required='true' maxlen='24'/>

	<parameter id='gpp_orden_atencion_ref' type='varchar' required='false' maxlen='2'/>
	<parameter id='gpp_area_servicio_ref' type='integer' required='false' maxlen='10'/>
	<parameter id='gpp_unidad_servicio_ref' type='integer' required='false' maxlen='10'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='gpp_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='gpp_descripcion'/>

	<custom-validator 
		classname='dinamica.validators.GetColValueValidator' 
		on-error-label='${lbl:b_field_does_not_registered}: gpp_orden_atencion' 
		parameter='gpp_orden_atencion_ref' 
		sql='gpp_orden_atencion.sql' 
		id='gpp_orden_atencion_ref'/>

	<custom-validator 
		classname='dinamica.validators.GetColValueValidator' 
		on-error-label='${lbl:b_field_does_not_registered}: gpp_area_servicio' 
		parameter='gpp_area_servicio_ref' 
		sql='gpp_area_servicio.sql' 
		id='gpp_area_servicio_ref'/>

	<custom-validator 
		classname='dinamica.validators.GetColValueValidator' 
		on-error-label='${lbl:b_field_does_not_registered}: gpp_unidad_servicio' 
		parameter='gpp_unidad_servicio_ref' 
		sql='gpp_unidad_servicio.sql' 
		id='gpp_unidad_servicio_ref'/>

</validator>
