	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>template.htm</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/html</content-type>
		<print mode='table' recordset='query-smn_grupo_componente_rf.sql' tag='smn_grupo_componente_rf_rows'/><print mode='table' recordset='query-cmp_clase_auxiliar_rf.sql' tag='cmp_clase_auxiliar_rf_rows'/>
