<?xml version='1.0' encoding='ISO-8859-1'?>
<config>

	<summary>
		${lbl:b_components}
	</summary>

	<log>false</log>
	
	<set-request-attribute id="progress" value="file:/WEB-INF/action/progress/progress.htm"/>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>true</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id='query-smn_grupo_componente_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-cmp_clase_auxiliar_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_unidad_medida_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_almacen_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_item_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_moneda_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_grupo_titulo_impresion_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_sub_tipo_gasto_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_tipo_gasto_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_servicios_rf.sql' source='sql' scope='transaction'/>
		<recordset id='query-smn_grupo_prestador_id.sql' source='sql' scope='transaction'/>
	</transaction>

	<output>
		<classname>dinamica.GenericOutput</classname>
		<template>template.htm</template>
		<set-http-headers>true</set-http-headers>
		<content-type>text/html</content-type>
		<print mode='table' recordset='query-smn_grupo_titulo_impresion_rf.sql' tag='smn_grupo_titulo_impresion_rf_rows'/>
		<print mode='table' recordset='query-smn_grupo_componente_rf.sql' tag='smn_grupo_componente_rf_rows'/>
		<print mode='table' recordset='query-cmp_clase_auxiliar_rf.sql' tag='cmp_clase_auxiliar_rf_rows'/>
		<print mode='table' recordset='query-smn_unidad_medida_rf.sql' tag='smn_unidad_medida_rf_rows'/>
		<print mode='table' recordset='query-smn_almacen_rf.sql' tag='smn_almacen_rf_rows'/>
		<print mode='table' recordset='query-smn_item_rf.sql' tag='smn_item_rf_rows'/>
		<print mode='table' recordset='query-smn_moneda_rf.sql' tag='smn_moneda_rf_rows'/>
		<print mode='table' recordset='query-smn_sub_tipo_gasto_rf.sql' tag='smn_sub_tipo_gasto_rf_rows'/>
		<print mode='table' recordset='query-smn_tipo_gasto_rf.sql' tag='smn_tipo_gasto_rf_rows'/>
		<print mode='table' recordset='query-smn_servicios_rf.sql' tag='smn_servicios_rf_rows'/>
		<print mode='table' recordset='query-smn_grupo_prestador_id.sql' tag='smn_grupo_prestador_id_rows'/>
		<print mode="form" recordset="_request" /> 
	</output>

</config>
