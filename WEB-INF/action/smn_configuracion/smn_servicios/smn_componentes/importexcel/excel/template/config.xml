<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smncomponentes.sql" source="session" scope="transaction" />
		<recordset id='query-smnclaseauxiliar.sql' source='sql' scope='transaction' />
	
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_components}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_components}">
				<col id='cmp_codigo' label='${lbl:b_code}' />
		<col id='cmp_descripcion' label='${lbl:b_description}' />
		<col id='cmp_tipo_componente

b_access_control' label='${lbl:b_type_component}' />
		<col id='smn_grupo_componente_rf

b_access_control' label='${lbl:b_group_component}' />
		<col id='cmp_forma_calculo

b_access_control' label='${lbl:b_calculus_form}' />
		<col id='cmp_vigencia_desde' label='${lbl:b_validation_date_from}' />
		<col id='cmp_vigencia_hasta' label='${lbl:b_validation_date_to}' />
		<col id='cmp_visible

b_access_control' label='${lbl:b_visible}' />
		<col id='cmp_clase_auxiliar_rf

b_access_control' label='${lbl:b_auxiliary_class}' />
		<col id='cmp_req_grupo_prestador

b_access_control' label='${lbl:b_require_providers_group}' />
		<col id='cmp_grupo_prestador

b_access_control' label='${lbl:b_group_of_provider}' />
		<col id='cmp_tiempo_estimado' label='${lbl:b_estimated_time}' />
		<col id='smn_unidad_medida_rf

b_access_control' label='${lbl:b_measurement_unit}' />
		<col id='smn_almacen_rf

b_access_control' label='${lbl:b_store}' />
		<col id='smn_item_rf

b_access_control' label='${lbl:b_item}' />
		<col id='cmp_porcentaje' label='${lbl:b_percentage}' />
		<col id='cmp_cantidad' label='${lbl:b_amount}' />
		<col id='cmp_precio_moneda_local' label='${lbl:b_coin_local_price}' />
		<col id='cmp_monto_moneda_local' label='${lbl:b_coin_local_amount}' />
		<col id='smn_moneda_rf

b_access_control' label='${lbl:b_coin}' />
		<col id='cmp_precio_moneda_alterna' label='${lbl:b_coin_altern_price}' />
		<col id='cmp_monto_moneda_alterna' label='${lbl:b_coin_altern_amount}' />
		<col id='cmp_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
		<sheet recordset='query-smnclaseauxiliar.sql' sheetname=''>
		<col id='cla_nombre' label='${lbl:b_name}' />
	</sheet>


	</excel>

</config>
