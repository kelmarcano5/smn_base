<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='stg_codigo' type='varchar' required='true' maxlen='16'/>
	<parameter id='stg_descripcion' type='varchar' required='true' maxlen='64'/>
	<parameter id='smn_tipo_gasto_id' type='integer' required='false' maxlen='10'/>


	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='stg_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='stg_descripcion'/>


</validator>
