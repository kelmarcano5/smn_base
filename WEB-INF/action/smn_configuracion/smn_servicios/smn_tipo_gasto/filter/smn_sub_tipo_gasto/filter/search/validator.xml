<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='stg_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='stg_codigo' rule='like' />
	<parameter id='stg_descripcion' type='varchar' required='false' label='${lbl:b_description}' maxlen='64'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='stg_descripcion' rule='like' />
	<parameter id='smn_tipo_gasto_id_name' type='varchar' required='false' label='${lbl:b_type_spent}'/>
	<parameter id='smn_tipo_gasto_id' type='integer' required='false' label='${lbl:b_type_spent}' maxlen='10'/>
	<parameter id='stg_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='stg_fecha_registro_ini' date2='stg_fecha_registro_fin' id='stg_fecha_registro_ini' />
	<parameter id='stg_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
