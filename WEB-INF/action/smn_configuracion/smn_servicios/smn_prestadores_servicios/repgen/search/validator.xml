<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='repgen.filter' onerror='/action/error/validation/ajax'>

	<parameter id='fdesde' type='date' required='false' label='Fecha desde'/>
	<parameter id='fhasta' type='date' required='false' label='Fecha hasta'/>
	
	<parameter id='prs_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<parameter id='prs_nombres' type='varchar' required='false' label='${lbl:s_profile_fname}' maxlen='32'/>
	<parameter id='prs_apellidos' type='varchar' required='false' label='${lbl:s_profile_lname}' maxlen='32'/>
	<parameter id='prs_area_servicio' type='integer' required='false' label='${lbl:b_service_area}' maxlen='10'/>
	<parameter id='prs_unidad_servicio' type='integer' required='false' label='${lbl:b_service_unit}' maxlen='10'/>
	<parameter id='prs_clase_auxiliar' type='integer' required='false' label='${lbl:b_auxiliary_class}' maxlen='10'/>
	<parameter id='prs_auxiliar' type='integer' required='false' label='${lbl:b_auxiliary}' maxlen='10'/>
	<parameter id='smn_grupos_prestadores' type='integer' required='false' label='${lbl:b_group_of_provider}' maxlen='10'/>
	<parameter id='prs_direccion' type='integer' required='false' label='${lbl:b_address}' maxlen='10'/>
	<parameter id='prs_telefono_fijo' type='varchar' required='false' label='${lbl:b_fixed_phone}' maxlen='24'/>
	<parameter id='prs_telefono_movil' type='varchar' required='false' label='${lbl:b_mobile_phone}' maxlen='24'/>
	<parameter id='prs_frecuencia' type='double' required='false' label='${lbl:b_frecuency}' maxlen='10'/>

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='prs_codigo'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='prs_nombres'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='prs_apellidos'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='prs_telefono_fijo'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='prs_telefono_movil'
		rule='contains' />


	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='prs_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarNombre.sql'
			id='prs_nombres'/>



	<custom-validator
			classname='dinamica.validators.ArrayNotEmptyValidator'
			on-error-label='Seleccione al menos un campo para la generación del reporte.'
			parameter-name='field_id' 
			id='field_id' />

	<custom-validator 
			classname='dinamica.validators.ArrayToRecordset'
			paramName='field_id' 
			recordsetId='fields' 
			colName='field_id' 
			colType='integer' />
				
</validator>
