<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='tda_codigo' type='varchar' required='true' maxlen='16'/>
	<parameter id='tda_descripción' type='varchar' required='true' maxlen='48'/>
	<parameter id='tda_estatus' type='varchar' required='true' maxlen='2'/>
	<parameter id='tda_vigencia' type='date' required='true' maxlen='10'/>


	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='tda_codigo'/>


</validator>
