<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="3" cellStep="0" name="${lbl:b_relacion_servicio_tipo_doc_anexo}"/>
	
	<excel 
		recordset='query.sql' 
		sheetname='${lbl:b_relacion_servicio_tipo_doc_anexo}' 
		filename='${lbl:b_relacion_servicio_tipo_doc_anexo}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='smn_servicios_id_combo' label='${lbl:b_service}' />
		<col id='smn_tipo_doc_anexo_id_combo' label='${lbl:b_tipo_documento_anexo}' />
		<col id='rst_idioma' label='${lbl:s_profile_lang}' />
		<col id='rst_usuario' label='${lbl:b_users}' />
		<col id='rst_fecha_registro' label='${lbl:b_store_date}' />
		<col id='rst_hora' label='${lbl:s_loginhist_col2}' />

	</excel>

</config>
