<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='gti_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='gti_codigo' rule='like' />
	<parameter id='gti_descripcion' type='varchar' required='false' label='${lbl:b_description}' maxlen='48'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='gti_descripcion' rule='like' />
	<parameter id='gti_estatus' type='varchar' required='false' label='${lbl:b_status}' maxlen='2'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='gti_estatus' rule='like' />
	<parameter id='gti_fecha_vigencia_ini' type='date' required='false' label='${lbl:b_validation_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_validation_date} mayor o igual que] no puede ser mayor que [${lbl:b_validation_date} menor o igual que].' date1='gti_fecha_vigencia_ini' date2='gti_fecha_vigencia_fin' id='gti_fecha_vigencia_ini' />
	<parameter id='gti_fecha_vigencia_fin' type='date' required='false' label='${lbl:b_validation_date}' maxlen='10'/>
	<parameter id='gti_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='gti_fecha_registro_ini' date2='gti_fecha_registro_fin' id='gti_fecha_registro_ini' />
	<parameter id='gti_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
