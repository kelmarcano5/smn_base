<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnrelcuestionarioseccion.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lbl:b_smn_rel_cuestionario_seccion}</pdf-title>
	<pdf-logo url='/images/logo-simone-pdf.png' scale='100' />
	<pdf-file-name>${lbl:b_smn_rel_cuestionario_seccion}.pdf</pdf-file-name>

	<pdf-report 
		pageSize="letter" 
		rotate="false"
		footer="false" 
		header="true">
		<record recordset='query-smnrelcuestionarioseccion.sql' width='100' spacingBefore='40'>
			<col name='cue_codigo_pl0' title='${lbl:b_smn_cuestionario_id}' width='50' align='left' />
			<col name='sec_codigo_pl1' title='${lbl:b_smn_seccion_id}' width='50' align='left' />

		</record>

	</pdf-report>
</config>
