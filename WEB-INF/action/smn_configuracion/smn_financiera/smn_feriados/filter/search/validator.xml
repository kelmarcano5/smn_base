<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='fer_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='fer_codigo' rule='like' />
	<parameter id='fer_descripcion' type='varchar' required='false' label='${lbl:b_description}' maxlen='48'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='fer_descripcion' rule='contains' />
	<parameter id='fer_pais' type='integer' required='false' label='${lbl:b_country}' maxlen='10'/>
	<parameter id='fer_entidad_federal' type='integer' required='false' label='${lbl:b_federal_entities}' maxlen='10'/>
	<parameter id='fer_ciudad' type='integer' required='false' label='${lbl:b_city}' maxlen='10'/>
	<parameter id='fer_municipio' type='integer' required='false' label='${lbl:b_municipality}' maxlen='10'/>
	<parameter id='fer_laborable' type='varchar' required='false' label='${lbl:b_workable}' maxlen='1'/>
	<parameter id='fer_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date_grater_than}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date_grater_than} mayor o igual que] no puede ser mayor que [${lbl:b_store_date_less_than} menor o igual que].' date1='fer_fecha_registro_ini' date2='fer_fecha_registro_fin' id='fer_fecha_registro_ini' />
	<parameter id='fer_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date_less_than}' maxlen='10'/>
</validator>
