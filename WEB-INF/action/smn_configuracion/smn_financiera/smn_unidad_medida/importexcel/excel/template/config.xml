<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnunidadmedida.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_measurement_unit}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_measurement_unit}">
				<col id='unm_codigo' label='${lbl:b_code}' />
		<col id='unm_descripcion' label='${lbl:b_description}' />
		<col id='smn_tipo_unidad_medida_rf' label='${lbl:b_measurement_unit_type}' />
		<col id='unm_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
	
	</excel>

</config>
