<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='unm_codigo' type='varchar' required='true' maxlen='16'/>
	<parameter id='unm_descripcion' type='varchar' required='true' maxlen='100'/>
	<parameter id='smn_tipo_unidad_medida_rf' type='integer' required='true' maxlen='10'/>


	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='unm_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='unm_descripcion'/>


</validator>
