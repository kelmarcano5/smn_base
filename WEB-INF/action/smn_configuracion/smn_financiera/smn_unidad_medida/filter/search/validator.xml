<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='unm_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='unm_codigo' rule='like' />
	<parameter id='unm_descripcion' type='varchar' required='false' label='${lbl:b_description}' maxlen='100'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='unm_descripcion' rule='like' />
	<parameter id='smn_tipo_unidad_medida_rf' type='integer' required='false' label='${lbl:b_measurement_unit_type}' maxlen='10'/>
	<parameter id='unm_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='unm_fecha_registro_ini' date2='unm_fecha_registro_fin' id='unm_fecha_registro_ini' />
	<parameter id='unm_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
