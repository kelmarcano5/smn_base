<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='tve_codigo' type='varchar' required='true' maxlen='16'/>
	<parameter id='tve_descripcion' type='varchar' required='true' maxlen='100'/>
	<parameter id='tve_estatus' type='varchar' required='true' maxlen='1'/>
	<parameter id='tve_vigencia' type='date' required='true' maxlen='10'/>


	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='tve_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='tve_descripcion'/>


</validator>
