<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnvencimientodetalle.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="3" cellStep="0" name="${lbl:b_detail_expiration}"/>
	
	<excel 
		recordset='query-smnvencimientodetalle.sql' 
		sheetname='${lbl:b_detail_expiration}' 
		filename='${lbl:b_detail_expiration}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='smn_vencimiento_cabecera_id' label='${lbl:b_header_expiration}' />
		<col id='tvd_titulo' label='${lbl:b_tittle}' />
		<col id='tvd_tipo_aplicacion' label='${lbl:b_application}' />
		<col id='tvd_dias_desde' label='${lbl:b_day_init}' />
		<col id='tvd_dias_hasta' label='${lbl:b_day_end}' />
		<col id='tvd_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
