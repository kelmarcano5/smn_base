<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='inc_codigo' type='varchar' required='true' maxlen='16'/>
	<parameter id='inc_descripcion' type='varchar' required='true' maxlen='64'/>
	<parameter id='smn_vencimiento_cabecera_id' type='integer' required='false' maxlen='10'/>
	<parameter id='inc_apli_cant_precio' type='varchar' required='true' maxlen='2'/>
	<parameter id='inc_porcentaje_base' type='double' required='true' maxlen='17'/>
	<parameter id='inc_porcentaje_incremento' type='double' required='true' maxlen='17'/>
	<parameter id='inc_apli_todos_it' type='varchar' required='false' maxlen='2'/>
	<parameter id='inc_apli_todos_se' type='varchar' required='false' maxlen='2'/>
	<parameter id='inc_apli_todos_af' type='varchar' required='false' maxlen='2'/>
	<parameter id='inc_estatus' type='varchar' required='true' maxlen='2'/>


	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='inc_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='inc_descripcion'/>


</validator>
