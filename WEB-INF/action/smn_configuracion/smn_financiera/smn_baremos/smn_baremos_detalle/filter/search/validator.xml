<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_baremos_id' type='integer' required='false' label='${lbl:b_baremo}' maxlen='10'/>
	<parameter id='smn_servicios_rf' type='integer' required='false' label='${lbl:b_service}' maxlen='10'/>
		<parameter id='smn_item_rf' type='integer' required='false' label='${lbl:b_item}' maxlen='10'/>
	<parameter id='bad_tipo_componente' type='varchar' required='false' label='${lbl:b_type}' maxlen='2'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='bad_tipo_componente' rule='like' />
	<parameter id='bad_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='bad_fecha_registro_ini' date2='bad_fecha_registro_fin' id='bad_fecha_registro_ini' />
	<parameter id='bad_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<parameter id='ite_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='ite_codigo' rule='contains' />
	<parameter id='ite_descripcion' type='varchar' required='false' label='${lbl:b_description}' maxlen='48'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='ite_descripcion' rule='like' />

	<parameter id='svc_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='svc_codigo' rule='contains' />
	<parameter id='svc_descripcion' type='varchar' required='false' label='${lbl:b_description}' maxlen='48'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='svc_descripcion' rule='like' />

	<parameter id="id2" type="integer" required="false" label="ID2" maxlen="50" />
</validator>
