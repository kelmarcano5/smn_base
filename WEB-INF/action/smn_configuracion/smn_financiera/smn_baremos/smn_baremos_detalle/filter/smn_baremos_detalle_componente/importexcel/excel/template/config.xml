<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnbaremosdetallecomponente.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_baremo_detail_component}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_baremo_detail_component}">
				<col id='smn_baremos_detalle_id_combo' label='${lbl:b_baremos_detail}' />
		<col id='smn_tipo_compuesto_rf_combo' label='${lbl:b_type}' />
		<col id='smn_componente_rf_combo' label='${lbl:b_components}' />
		<col id='smn_almacen_rf_combo' label='${lbl:b_store_code}' />
		<col id='smn_item_rf_combo' label='${lbl:b_item}' />
		<col id='smn_forma_calculo_rf_combo' label='${lbl:b_calculus_form}' />
		<col id='smn_porcentaje_rf' label='${lbl:b_percentage}' />
		<col id='bdc_cantidad' label='${lbl:b_quantity}' />
		<col id='bdc_precio_ml' label='${lbl:b_price}' />
		<col id='bdc_monto_ml' label='${lbl:b_amount}' />
		<col id='smn_moneda_rf_combo' label='${lbl:b_coin}' />
		<col id='bdc_precio_ma' label='${lbl:b_coin_altern_price}' />
		<col id='bdc_monto_ma' label='${lbl:b_coin_altern_amount}' />
		<col id='bdc_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
	
	</excel>

</config>
