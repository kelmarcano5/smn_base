<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="3" cellStep="0" name="${lbl:b_baremos}"/>
	
	<excel 
		recordset='query.sql' 
		sheetname='${lbl:b_baremos}' 
		filename='${lbl:b_baremos}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='bar_codigo' label='${lbl:b_code}' />
		<col id='bar_descripcion' label='${lbl:b_description}' />
		<col id='smn_entidades_rf' label='${lbl:b_entity}' />
		<col id='smn_sucursales_rf' label='${lbl:b_subsidiary}' />
		<col id='bar_vigencia_desde' label='${lbl:b_validation_date_from}' />
		<col id='bar_vigencia_hasta' label='${lbl:b_validation_date_to}' />
		<col id='bar_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
