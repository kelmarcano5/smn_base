<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="3" cellStep="0" name="${lbl:b_way_to_pay}"/>
	
	<excel 
		recordset='query.sql' 
		sheetname='${lbl:b_way_to_pay}' 
		filename='${lbl:b_way_to_pay}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='fop_codigo' label='${lbl:b_code}' />
		<col id='fop_descripcion' label='${lbl:b_description}' />
		<col id='mpa_descripcion' label='${lbl:b_medio_pago}' />
		<col id='fop_requiere_banco_combo' label='${lbl:b_bank}' />
		<col id='fra_descripcion' label='${lbl:b_franquicias}' />
		<col id='fop_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
