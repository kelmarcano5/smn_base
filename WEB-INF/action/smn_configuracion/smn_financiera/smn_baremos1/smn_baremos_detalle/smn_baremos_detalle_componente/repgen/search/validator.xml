<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='repgen.filter' onerror='/action/error/validation/ajax'>

	<parameter id='fdesde' type='date' required='false' label='Fecha desde'/>
	<parameter id='fhasta' type='date' required='false' label='Fecha hasta'/>
	
	<parameter id='smn_baremos_detalle_id' type='integer' required='false' label='${lbl:b_baremos_detail}' maxlen='10'/>
	<parameter id='smn_tipo_compuesto_rf' type='varchar' required='false' label='${lbl:b_type}' maxlen='1'/>
	<parameter id='smn_componente_rf' type='integer' required='false' label='${lbl:b_components}' maxlen='10'/>
	<parameter id='smn_almacen_rf' type='integer' required='false' label='${lbl:b_store_code}' maxlen='10'/>
	<parameter id='smn_item_rf' type='integer' required='false' label='${lbl:b_item}' maxlen='10'/>
	<parameter id='smn_forma_calculo_rf' type='varchar' required='false' label='${lbl:b_calculus_form}' maxlen='1'/>
	<parameter id='smn_porcentaje_rf' type='double' required='false' label='${lbl:b_percentage}' maxlen='10'/>
	<parameter id='bdc_cantidad' type='integer' required='false' label='${lbl:b_quantity}' maxlen='10'/>
	<parameter id='bdc_precio_ml' type='double' required='false' label='${lbl:b_price}' maxlen='10'/>
	<parameter id='bdc_monto_ml' type='double' required='false' label='${lbl:b_amount}' maxlen='10'/>
	<parameter id='smn_moneda_rf' type='integer' required='false' label='${lbl:b_coin}' maxlen='10'/>
	<parameter id='bdc_precio_ma' type='double' required='false' label='${lbl:b_coin_altern_price}' maxlen='10'/>
	<parameter id='bdc_monto_ma' type='double' required='false' label='${lbl:b_coin_altern_amount}' maxlen='10'/>

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='smn_tipo_compuesto_rf'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='smn_forma_calculo_rf'
		rule='contains' />



	<custom-validator
			classname='dinamica.validators.ArrayNotEmptyValidator'
			on-error-label='Seleccione al menos un campo para la generación del reporte.'
			parameter-name='field_id' 
			id='field_id' />

	<custom-validator 
			classname='dinamica.validators.ArrayToRecordset'
			paramName='field_id' 
			recordsetId='fields' 
			colName='field_id' 
			colType='integer' />
				
</validator>
