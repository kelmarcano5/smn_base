<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_baremos_detalle_id' type='integer' required='false' label='${lbl:b_baremos_detail}' maxlen='10'/>
	<parameter id='smn_tipo_compuesto_rf' type='varchar' required='false' label='${lbl:b_type}' maxlen='1'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='smn_tipo_compuesto_rf' rule='like' />
	<parameter id='smn_componente_rf' type='integer' required='false' label='${lbl:b_components}' maxlen='10'/>
	<parameter id='bdc_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='bdc_fecha_registro_ini' date2='bdc_fecha_registro_fin' id='bdc_fecha_registro_ini' />
	<parameter id='bdc_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
