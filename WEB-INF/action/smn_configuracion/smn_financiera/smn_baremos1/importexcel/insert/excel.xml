<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='bar_codigo' type='varchar' required='true' maxlen='16'/>
	<parameter id='bar_descripcion' type='varchar' required='true' maxlen='48'/>
	<parameter id='smn_entidades_rf' type='integer' required='true' maxlen='2'/>
	<parameter id='smn_sucursales_rf' type='integer' required='false' maxlen='10'/>
	<parameter id='bar_vigencia_desde' type='date' required='true' maxlen='10'/>
	<parameter id='bar_vigencia_hasta' type='date' required='true' maxlen='10'/>
	<parameter id='bar_tipo' type='varchar' required='true' maxlen='1'/>
	<parameter id='smn_clase_auxiliar_rf' type='integer' required='false' maxlen='10'/>
	<parameter id='smn_auxiliar_rf' type='integer' required='false' maxlen='10'/>


	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='bar_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='bar_descripcion'/>



</validator>
