<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnbaremosdetalle.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="4" cellStep="0" name="${lbl:b_baremos_detail}"/>
	
	<excel 
		recordset='query-smnbaremosdetalle.sql' 
		sheetname='${lbl:b_baremos_detail}' 
		filename='${lbl:b_baremos_detail}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='smn_baremos_id_combo' label='${lbl:b_baremo}' />
		<col id='bad_tipo_componente_combo' label='${lbl:b_type}' />
		<col id='producto' label='${lbl:b_description}' />
		<col id='bad_cantidad' label='${lbl:b_amount}' />
		<col id='bad_precio_moneda_local' label='${lbl:b_coin_local_price}' />
		<col id='bad_precio_moneda_alterna' label='${lbl:b_coin_altern_price}' />
		<col id='bad_estatus' label='${lbl:b_status}' />
		<col id='bad_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
