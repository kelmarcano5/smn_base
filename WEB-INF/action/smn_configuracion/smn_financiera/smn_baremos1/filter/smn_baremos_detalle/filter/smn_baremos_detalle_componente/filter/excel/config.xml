<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnbaremosdetallecomponente.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="4" cellStep="0" name="${lbl:b_baremo_detail_component}"/>
	
	<excel 
		recordset='query-smnbaremosdetallecomponente.sql' 
		sheetname='${lbl:b_baremo_detail_component}' 
		filename='${lbl:b_baremo_detail_component}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='smn_baremos_detalle_id_combo' label='${lbl:b_baremos_detail}' />
		<col id='smn_tipo_compuesto_rf_combo' label='${lbl:b_type}' />
		<col id='smn_componente_rf_combo' label='${lbl:b_components}' />
		<col id='smn_item_rf_combo' label='${lbl:b_item}' />
		<col id='bdc_cantidad' label='${lbl:b_quantity}' />
		<col id='bdc_precio_ml' label='${lbl:b_price}' />
		<col id='bdc_monto_ml' label='${lbl:b_amount}' />
		<col id='bdc_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
