<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='bar_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='bar_codigo' rule='like' />
	<parameter id='bar_descripcion' type='varchar' required='false' label='${lbl:b_description}' maxlen='48'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='bar_descripcion' rule='like' />
	<parameter id='smn_entidades_rf' type='integer' required='false' label='${lbl:b_entity}' maxlen='2'/>
	<parameter id='smn_sucursales_rf' type='integer' required='false' label='${lbl:b_subsidiary}' maxlen='10'/>
	<parameter id='bar_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='bar_fecha_registro_ini' date2='bar_fecha_registro_fin' id='bar_fecha_registro_ini' />
	<parameter id='bar_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
