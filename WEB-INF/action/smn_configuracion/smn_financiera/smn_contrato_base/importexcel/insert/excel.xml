<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='ctr_codigo' type='varchar' required='true' maxlen='16'/>
	<parameter id='ctr_nombre' type='varchar' required='true' maxlen='300'/>
	<parameter id='ctr_transaccion_rf_desc_ref' type='varchar' required='true' maxlen='10'/>
	<parameter id='ctr_tipo' type='varchar' required='true' maxlen='2'/>
	<parameter id='ctr_fecha_vencimiento' type='date' required='true' maxlen='10'/>
	<parameter id='ctr_corporacion_rf_desc_ref' type='varchar' required='true' maxlen='10'/>

	<parameter id='ctr_transaccion_rf_ref' type='integer' required='false' maxlen='10'/>
	<parameter id='ctr_corporacion_rf_ref' type='integer' required='false' maxlen='10'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='ctr_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarNombre.sql'
			id='ctr_nombre'/>


	<custom-validator 
		classname='dinamica.validators.GetColValueValidator' 
		on-error-label='${lbl:b_field_does_not_registered}: ctr_transaccion_rf' 
		parameter='ctr_transaccion_rf_ref' 
		sql='ctr_transaccion_rf.sql' 
		id='ctr_transaccion_rf_ref'/>

	<custom-validator 
		classname='dinamica.validators.GetColValueValidator' 
		on-error-label='${lbl:b_field_does_not_registered}: ctr_corporacion_rf' 
		parameter='ctr_corporacion_rf_ref' 
		sql='ctr_corporacion_rf.sql' 
		id='ctr_corporacion_rf_ref'/>

</validator>
