<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smndescuento.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="2" cellStep="0" name="${lbl:b_discount}"/>
	
	<excel 
		recordset='query-smndescuento.sql' 
		sheetname='${lbl:b_discount}' 
		filename='${lbl:b_discount}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='dct_codigo' label='${lbl:b_code}' />
		<col id='dct_nombre' label='${lbl:b_name}' />
		<col id='dct_porcentaje' label='${lbl:b_percentage}' />
		<col id='dct_aplica_combo' label='${lbl:b_apply}' />
		<col id='dct_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
