<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="false">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smncuentasbancarias-template.sql" source="sql" scope="transaction" />
		<recordset id='query-smnentidadesfinancieras.sql' source='sql' scope='transaction' />
		<recordset id='query-smnmonedas.sql' source='sql' scope='transaction' />
		<recordset id='query-smntipocuentabanco.sql' source='sql' scope='transaction' />
	</transaction>

	<output>
		<classname>domain.repgen.smn_configuracion.smn_financiera.smn_cuentas_bancarias.ExcelReportOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_bank_accounts}.xls'     
		header-background='true' >
		
	<sheet recordset="query-smncuentasbancarias-template.sql" sheetname="${lbl:b_bank_accounts}">
		
		<col id='cba_entidades_financieras' label='${lbl:b_financial_entity}' />
		<col id='cba_codigo' label='${lbl:b_code}' />
		<col id='cba_nombre' label='${lbl:b_name}' />
		<col id='cba_nro_cuenta' label='${lbl:b_bank_account_number}' />
		<col id='cba_tipo_cta_banco' label='${lbl:b_account_bank_type}' />
		<col id='cba_moneda' label='${lbl:b_coin}' />
		
		<col id='cba_entidades_financieras_reference' label='${lbl:b_financial_entity}' />
		<col id='cba_tipo_cta_banco_reference' label='${lbl:b_account_bank_type}' />
		<col id='cba_moneda_reference' label='${lbl:b_coin}' />

	</sheet>
		<sheet recordset='query-smnentidadesfinancieras.sql' sheetname='${lbl:b_financial_entity}'>
		<col id='efi_nombre' label='${lbl:b_name}' />
	</sheet>

	<sheet recordset='query-smnmonedas.sql' sheetname='${lbl:b_coin}'>
		<col id='mon_nombre' label='${lbl:b_name}' />
	</sheet>

	<sheet recordset='query-smntipocuentabanco.sql' sheetname='${lbl:b_account_bank_type}'>
		<col id='tcb_descripcion' label='${lbl:b_description}' />
	</sheet>


	</excel>

</config>
