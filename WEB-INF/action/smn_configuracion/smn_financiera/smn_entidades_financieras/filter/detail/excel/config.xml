<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="6" cellStep="0" name="${lbl:b_financial_entities}"/>
	
	<excel 
		recordset='query.sql' 
		sheetname='${lbl:b_financial_entities}' 
		filename='${lbl:b_financial_entities}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='efi_codigo' label='${lbl:b_code}' />
		<col id='efi_nombre' label='${lbl:b_name}' />
		<col id='dir_descripcion' label='${lbl:b_address}' />
		<col id='efi_telefono_fijo' label='${lbl:b_fixed_phone}' />
		<col id='efi_telefono_movil' label='${lbl:b_mobile_phone}' />
		<col id='efi_nombre' label='${lbl:b_aba_code}' />
		<col id='efi_nombre' label='${lbl:b_swift_code}' />
		<col id='efi_req_banco_intermediario_combo' label='${lbl:b_req_intermediary_institution}' />
		<col id='efi_nombre' label='${lbl:b_intermediary_institution}' />
		<col id='efi_routing' label='${lbl:b_routing_code}' />
		<col id='efi_contacto' label='${lbl:b_account_executive}' />
		<col id='efi_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
