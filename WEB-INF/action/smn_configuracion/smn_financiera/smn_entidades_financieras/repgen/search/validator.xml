<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='repgen.filter' onerror='/action/error/validation/ajax'>

	<parameter id='fdesde' type='date' required='false' label='Fecha desde'/>
	<parameter id='fhasta' type='date' required='false' label='Fecha hasta'/>
	
	<parameter id='efi_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<parameter id='efi_nombre' type='varchar' required='false' label='${lbl:b_name}' maxlen='48'/>
	<parameter id='efi_direccion' type='integer' required='false' label='${lbl:b_address}' maxlen='10'/>
	<parameter id='efi_telefono_fijo' type='varchar' required='false' label='${lbl:b_fixed_phone}' maxlen='24'/>
	<parameter id='efi_telefono_movil' type='varchar' required='false' label='${lbl:b_mobile_phone}' maxlen='24'/>
	<parameter id='efi_codigo_aba' type='varchar' required='false' label='${lbl:b_aba_code}' maxlen='24'/>
	<parameter id='efi_codigo_swift' type='varchar' required='false' label='${lbl:b_swift_code}' maxlen='24'/>
	<parameter id='efi_req_banco_intermediario' type='varchar' required='false' label='${lbl:b_req_intermediary_institution}' maxlen='1'/>
	<parameter id='efi_banco_intermediario' type='integer' required='false' label='${lbl:b_intermediary_institution}' maxlen='10'/>
	<parameter id='efi_routing' type='varchar' required='false' label='${lbl:b_routing_code}' maxlen='24'/>
	<parameter id='efi_contacto' type='varchar' required='false' label='${lbl:b_account_executive}' maxlen='48'/>
	
	<parameter id='efi_codigo_filter' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<parameter id='efi_nombre_filter' type='varchar' required='false' label='${lbl:b_name}' maxlen='48'/>
	<parameter id='efi_req_banco_intermediario_filter' type='varchar' required='false' label='${lbl:b_req_intermediary_institution}' maxlen='1'/>
	<parameter id='efi_banco_intermediario_filter' type='integer' required='false' label='${lbl:b_intermediary_institution}' maxlen='10'/>
	<parameter id='efi_contacto_filter' type='varchar' required='false' label='${lbl:b_account_executive}' maxlen='48'/>

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='efi_codigo_filter'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='efi_nombre_filter'
		rule='contains' />
		
	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='efi_req_banco_intermediario_filter'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='efi_contacto_filter'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='efi_codigo'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='efi_nombre'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='efi_telefono_fijo'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='efi_telefono_movil'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='efi_codigo_aba'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='efi_codigo_swift'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='efi_req_banco_intermediario'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='efi_routing'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='efi_contacto'
		rule='contains' />


	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='efi_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarNombre.sql'
			id='efi_nombre'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='efi_codigo_aba'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='efi_codigo_swift'/>


	<custom-validator
			classname='dinamica.validators.ArrayNotEmptyValidator'
			on-error-label='Seleccione al menos un campo para la generación del reporte.'
			parameter-name='field_id' 
			id='field_id' />

	<custom-validator 
			classname='dinamica.validators.ArrayToRecordset'
			paramName='field_id' 
			recordsetId='fields' 
			colName='field_id' 
			colType='integer' />
				
</validator>
