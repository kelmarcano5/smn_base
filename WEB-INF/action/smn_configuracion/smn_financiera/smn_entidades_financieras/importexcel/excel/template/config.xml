<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="false">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnentidadesfinancieras-template.sql" source="sql" scope="transaction" />
		<recordset id='query-smnentidadesfinancieras.sql' source='session' scope='transaction' />
		<recordset id='query-smndireccion.sql' source='sql' scope='transaction' />
		<recordset id='query-reqbancointermediario.sql' source='sql' scope='transaction' />
	</transaction>

	<output>
		<classname>domain.repgen.smn_configuracion.smn_financiera.smn_entidades_financieras.ExcelReportOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_financial_entities}.xls'  
		header-background='true' >
		
	<sheet recordset="query-smnentidadesfinancieras-template.sql" sheetname="${lbl:b_financial_entities}">
		
		<col id='efi_codigo' label='${lbl:b_code}' />
		<col id='efi_nombre' label='${lbl:b_name}' />
		<col id='efi_direccion' label='${lbl:b_address}' />
		<col id='efi_telefono_fijo' label='${lbl:b_fixed_phone}' />
		<col id='efi_telefono_movil' label='${lbl:b_mobile_phone}' />
		<col id='efi_codigo_aba' label='${lbl:b_aba_code}' />
		<col id='efi_codigo_swift' label='${lbl:b_swift_code}' />
		<col id='efi_req_banco_intermediario_combo' label='${lbl:b_req_intermediary_institution}' />
		<col id='efi_banco_intermediario' label='${lbl:b_intermediary_institution}' />
		<col id='efi_routing' label='${lbl:b_routing_code}' />
		<col id='efi_contacto' label='${lbl:b_account_executive}' />
		
		<col id='efi_direccion_reference' label='${lbl:b_address}' />
		<col id='efi_req_banco_intermediario_combo_reference' label='${lbl:b_req_intermediary_institution}' />
		<col id='efi_banco_intermediario_reference' label='${lbl:b_intermediary_institution}' />

	</sheet>
	
	<sheet recordset='query-reqbancointermediario.sql' sheetname='${lbl:b_req_intermediary_institution}'>
		<col id='req_banco' label='${lbl:b_req_intermediary_institution}' />
	</sheet>
	
	<sheet recordset='query-smnentidadesfinancieras.sql' sheetname='${lbl:b_intermediary_institution}'>
		<col id='efi_nombre' label='${lbl:b_name}' />
	</sheet>

	<sheet recordset='query-smndireccion.sql' sheetname='${lbl:b_address}'>
		<col id='dir_descripcion' label='${lbl:b_description}' />
	</sheet>


	</excel>

</config>
