<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='mpa_codigo' type='varchar' required='true' maxlen='16'/>
	<parameter id='mpa_descripcion' type='varchar' required='true' maxlen='48'/>
	<parameter id='mpa_tipo_medio_pago' type='varchar' required='true' maxlen='2'/>
	<parameter id='mpa_estatus' type='varchar' required='true' maxlen='2'/>
	<parameter id='mpa_vigencia' type='date' required='true' maxlen='10'/>


	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='mpa_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='mpa_descripcion'/>


</validator>
