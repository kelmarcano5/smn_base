	<parameter id='mpa_descripcion' type='varchar' required='false' label='${lbl:b_description}' maxlen='48'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='mpa_descripcion' rule='like' />
	<parameter id='mpa_tipo_medio_pago' type='varchar' required='false' label='${lbl:b_type_half_payment}' maxlen='2'/>
