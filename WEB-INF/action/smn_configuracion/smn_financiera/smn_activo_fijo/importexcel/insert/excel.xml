<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='acf_codigo' type='varchar' required='true' maxlen='16'/>
	<parameter id='acf_nombre' type='varchar' required='true' maxlen='64'/>
	<parameter id='acf_proveedor_exclusivo' type='varchar' required='false' maxlen='2'/>


	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='acf_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarNombre.sql'
			id='acf_nombre'/>



</validator>
