<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnactivofijo.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="1" cellStep="0" name="${lbl:b_fixed_assets}"/>
	
	<excel 
		recordset='query-smnactivofijo.sql' 
		sheetname='${lbl:b_fixed_assets}' 
		filename='${lbl:b_fixed_assets}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='acf_codigo' label='${lbl:b_code}' />
		<col id='acf_nombre' label='${lbl:b_name}' />
		<col id='acf_proveedor_exclusivo_combo' label='${lbl:b_provider_exclusive}' />

	</excel>

</config>
