<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnitem.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_item}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_item}">
				<col id='itm_codigo' label='${lbl:b_code}' />
		<col id='itm_nombre' label='${lbl:b_name}' />
		<col id='itm_proveedor_exclusivo_combo' label='${lbl:b_provider_exclusive}' />
		<col id='smn_almacenado_combo' label='${lbl:b_store}' />
		<col id='itm_codigo' label='${lbl:b_code}' />
		<col id='itm_nombre' label='${lbl:b_name}' />
		<col id='itm_proveedor_exclusivo_combo' label='${lbl:b_provider_exclusive}' />
		<col id='smn_almacenado_combo' label='${lbl:b_store}' />

	</sheet>
	
	</excel>

</config>
