<?xml version='1.0' encoding='ISO-8859-1'?>
<validator onerror='/action/error/validation/ajax'>
	<parameter id='tda_codigo' type='varchar' required='true' label='${lbl:b_code}' maxlen='16'/>
	<parameter id='tda_descripción' type='varchar' required='true' label='${lbl:b_description}' maxlen='48'/>
	<parameter id='tda_estatus' type='varchar' required='true' label='${lbl:b_status}' maxlen='2'/>
	<parameter id='tda_vigencia' type='date' required='true' label='${lbl:b_vigency}' maxlen='10'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='tda_codigo'/>


</validator>
