<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_estructura_codigo_id' type='integer' required='false' label='${lbl:b_estructura_codigo}' maxlen='16'/>
	<parameter id='nes_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
		<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='nes_codigo' rule='contains' />
	<parameter id='nes_niveles' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<parameter id='nes_tipo' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<parameter id='nes_nombre' type='varchar' required='false' label='${lbl:b_name}' maxlen='48'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='nes_nombre' rule='contains' />
	<parameter id='nes_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date_greather_than}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date_greather_than} mayor o igual que] no puede ser mayor que [${lbl:b_store_date_less_than} menor o igual que].' date1='nes_fecha_registro_ini' date2='nes_fecha_registro_fin' id='nes_fecha_registro_ini' />
	<parameter id='nes_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date_less_than}' maxlen='10'/>
</validator>
