<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnestructuracodigo.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_estructura_codigo}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_estructura_codigo}">
				<col id='smn_modulos_id_combo' label='${lbl:b_module}' />
		<col id='eco_codigo' label='${lbl:b_code}' />
		<col id='eco_nombre' label='${lbl:b_name}' />
		<col id='eco_niveles' label='${lbl:b_levels}' />
		<col id='eco_mascara' label='${lbl:b_mask}' />
		<col id='eco_separador' label='${lbl:b_separator}' />
		<col id='eco_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
	
	</excel>

</config>
