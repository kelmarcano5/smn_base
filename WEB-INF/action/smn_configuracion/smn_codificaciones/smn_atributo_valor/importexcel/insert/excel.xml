<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='smn_atributo_item_id' type='integer' required='false' maxlen='10'/>
	<parameter id='atv_codigo' type='varchar' required='true' maxlen='16'/>
	<parameter id='atv_descripcion' type='integer' required='true' maxlen='10'/>
	<parameter id='atv_fecha_vigencia_desde' type='date' required='true' maxlen='10'/>
	<parameter id='atv_fecha_vigencia_hasta' type='date' required='true' maxlen='10'/>


	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='atv_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='atv_descripcion'/>


</validator>
