<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="1" cellStep="0" name="${lbl:b_relacion_nivel_estructura_atributo_item}"/>
	
	<excel 
		recordset='query.sql' 
		sheetname='${lbl:b_relacion_nivel_estructura_atributo_item}' 
		filename='${lbl:b_relacion_nivel_estructura_atributo_item}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='smn_nivel_estructura_id_combo' label='${lbl:b_nivel_estructura}' />
		<col id='smn_atributo_item_id_combo' label='${lbl:b_atributos_item}' />
		<col id='rna_secuencia' label='${lbl:b_sequence}' />

	</excel>

</config>
