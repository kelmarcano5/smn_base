<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="false">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnestructurauen-template.sql" source="sql" scope="transaction" />
		<recordset id="query-uen.sql" source="sql" scope="transaction" />
		<recordset id="query-uenauxiliary.sql" source="sql" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.repgen.smn_configuracion.smn_general.smn_estructura_uen.ExcelReportOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_business_unit_structure}.xls'      
		header-background='true' >
		
	<sheet recordset="query-smnestructurauen-template.sql" sheetname="${lbl:b_business_unit_structure}">
	
		<col id='eue_aplica_uen_combo' label='${lbl:b_apply_business_unit}' />
		<col id='eue_aplica_auxiliar_uen_combo' label='${lbl:b_apply_auxiliary}' />
		
		<col id='eue_aplica_uen_combo_reference' label='${lbl:b_apply_business_unit}' />
		<col id='eue_aplica_auxiliar_uen_combo_reference' label='${lbl:b_apply_auxiliary}' />
		
	</sheet>

	<sheet recordset='query-uen.sql' sheetname='${lbl:b_apply_business_unit}'>
		<col id='uen' label='${lbl:b_apply_business_unit}' />
	</sheet>
	
	<sheet recordset='query-uenauxiliary.sql' sheetname='${lbl:b_apply_auxiliary}'>
		<col id='uen_auxiliary' label='${lbl:b_apply_auxiliary}' />
	</sheet>
	
	</excel>

</config>
