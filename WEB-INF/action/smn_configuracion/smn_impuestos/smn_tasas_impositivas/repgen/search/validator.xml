<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='repgen.filter' onerror='/action/error/validation/ajax'>

	<parameter id='fdesde' type='date' required='false' label='Fecha desde'/>
	<parameter id='fhasta' type='date' required='false' label='Fecha hasta'/>
	
	<parameter id='smn_unidades_impositivas_id_name' type='varchar' required='false' label='${lbl:b_tax_unit}'/>
	<parameter id='smn_unidades_impositivas_id' type='integer' required='false' label='${lbl:b_tax_unit}' maxlen='10'/>
	
	<parameter id='tim_fecha_vigencia_ini' type='date' required='false' label='${lbl:b_validation_date}' maxlen='10'/>
	<parameter id='tim_fecha_vigencia_fin' type='date' required='false' label='${lbl:b_validation_date}' maxlen='10'/>
	
	<parameter id='tim_valor_ini' type='double' required='false' label='${lbl:b_value}' maxlen='10'/>
	<parameter id='tim_valor_fin' type='double' required='false' label='${lbl:b_value}' maxlen='10'/>
	
	
	<parameter id='uim_id_filter' type='integer' required='false' label='${lbl:b_tax_unit}' maxlen='10'/>
	
	<parameter id='tim_fecha_vigencia_ini_filter' type='date' required='false' label='${lbl:b_validation_date}' maxlen='10'/>
	<parameter id='tim_fecha_vigencia_fin_filter' type='date' required='false' label='${lbl:b_validation_date}' maxlen='10'/>
	
	<parameter id='tim_valor_ini_filter' type='double' required='false' label='${lbl:b_value}' maxlen='10'/>
	<parameter id='tim_valor_fin_filter' type='double' required='false' label='${lbl:b_value}' maxlen='10'/>

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='smn_unidades_impositivas_id_name'
		rule='contains' />

	<custom-validator
			classname='dinamica.validators.ArrayNotEmptyValidator'
			on-error-label='${lbl:b_at_least_field_report}.'
			parameter-name='field_id' 
			id='field_id' />

	<custom-validator 
			classname='dinamica.validators.ArrayToRecordset'
			paramName='field_id' 
			recordsetId='fields' 
			colName='field_id' 
			colType='integer' />
				
</validator>
