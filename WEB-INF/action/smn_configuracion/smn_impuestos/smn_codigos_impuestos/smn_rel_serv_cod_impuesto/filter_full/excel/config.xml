<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnrelservcodimpuesto.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
	
	<excel recordset='query-smnrelservcodimpuesto.sql' sheetname='${lbl:b_relacion_servicio_codigos_impuestos}' filename='filter_full.xls'>
		<col id='smn_codigos_impuestos_id_combo' label='${lbl:b_taxes}' />
		<col id='smn_servicios_id_combo' label='${lbl:b_service}' />
		<col id='rsi_idioma' label='${lbl:s_profile_lang}' />
		<col id='rsi_usuario' label='${lbl:b_users}' />
		<col id='rsi_fecha_registro' label='${lbl:b_store_date}' />
		<col id='rsi_hora' label='${lbl:s_loginhist_col2}' />
	</excel>

</config>
