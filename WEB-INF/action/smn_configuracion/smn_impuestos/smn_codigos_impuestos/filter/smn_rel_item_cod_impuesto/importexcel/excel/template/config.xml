<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnrelitemcodimpuesto.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_relacion_item_codigos_impuestos}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_relacion_item_codigos_impuestos}">
				<col id='smn_item_id_combo' label='${lbl:b_item}' />
		<col id='smn_codigos_impuestos_id_combo' label='${lbl:b_taxes}' />
		<col id='rii_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
	
	</excel>

</config>
