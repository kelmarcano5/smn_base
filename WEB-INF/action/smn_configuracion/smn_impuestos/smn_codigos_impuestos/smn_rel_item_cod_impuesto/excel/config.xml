<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnrelitemcodimpuesto.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="3" cellStep="0" name="${lbl:b_relacion_item_codigos_impuestos}"/>
	
	<excel 
		recordset='query-smnrelitemcodimpuesto.sql' 
		sheetname='${lbl:b_relacion_item_codigos_impuestos}' 
		filename='${lbl:b_relacion_item_codigos_impuestos}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='smn_item_id_combo' label='${lbl:b_item}' />
		<col id='smn_codigos_impuestos_id_combo' label='${lbl:b_taxes}' />
		<col id='rii_idioma' label='${lbl:s_profile_lang}' />
		<col id='rii_usuario' label='${lbl:b_users}' />
		<col id='rii_fecha_registro' label='${lbl:b_store_date}' />
		<col id='rii_hora' label='${lbl:s_loginhist_col2}' />

	</excel>

</config>
