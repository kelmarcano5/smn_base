<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='uim_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='uim_codigo' rule='contains' />
	<parameter id='uim_descripcion' type='varchar' required='false' label='${lbl:b_description}' maxlen='48'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='uim_descripcion' rule='contains' />
	<parameter id='uim_moneda_referencia' type='integer' required='false' label='${lbl:b_referenced_coin}' maxlen='10'/>
	<parameter id='uim_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date_grater_than}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date_grater_than} mayor o igual que] no puede ser mayor que [${lbl:b_store_date_less_than} menor o igual que].' date1='uim_fecha_registro_ini' date2='uim_fecha_registro_fin' id='uim_fecha_registro_ini' />
	<parameter id='uim_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date_less_than}' maxlen='10'/>
</validator>
