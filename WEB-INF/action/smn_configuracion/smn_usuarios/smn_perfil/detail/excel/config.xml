<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="5" cellStep="0" name="${lbl:b_profile}"/>
	
	<excel 
		recordset='query.sql' 
		sheetname='${lbl:b_profile}' 
		filename='${lbl:b_profile}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='description' label='${lbl:b_role}' />
		<col id='description' label='${lbl:b_service}' />
		<col id='prf_tipo_acceso' label='${lbl:b_type_access}' />
		<col id='prf_add' label='${lbl:b_add}' />
		<col id='prf_del' label='${lbl:b_delete}' />
		<col id='prf_edit' label='${lbl:b_modify}' />
		<col id='prf_view' label='${lbl:b_view}' />
		<col id='prf_export' label='${lbl:b_export}' />
		<col id='prf_find' label='${lbl:b_find}' />
		<col id='prf_import' label='${lbl:b_import}' />
		<col id='prf_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
