<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='pca_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='pca_codigo' rule='like' />
	<parameter id='pca_descripcion' type='varchar' required='false' label='${lbl:b_description}' maxlen='500'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='pca_descripcion' rule='like' />
	<parameter id='smn_modulo_rf' type='integer' required='false' label='${lbl:b_modules}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='smn_modulo_rf' rule='like' />
	<parameter id='pca_status' type='varchar' required='false' label='${lbl:b_status}' maxlen='2'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='pca_status' rule='like' />
	<parameter id='pca_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date}@{${def:date}} mayor o igual que] no puede ser mayor que [${lbl:b_store_date}@{${def:date}} menor o igual que].' date1='pca_fecha_registro_ini' date2='pca_fecha_registro_fin' id='pca_fecha_registro_ini' />
	<parameter id='pca_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
