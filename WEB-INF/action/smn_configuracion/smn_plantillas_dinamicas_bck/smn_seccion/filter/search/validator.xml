<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='sec_codigo' type='varchar' required='false' label='${lbl:b_sec_codigo}' maxlen='16'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='sec_codigo' rule='like' />
	<parameter id='sec_descripcion' type='varchar' required='false' label='${lbl:b_sec_descripcion}' maxlen='100'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='sec_descripcion' rule='like' />
	<parameter id='sec_estatus' type='varchar' required='false' label='${lbl:b_status}' maxlen='2'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='sec_estatus' rule='like' />
	<parameter id='sec_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date}@{${def:date}}
 mayor o igual que] no puede ser mayor que [${lbl:b_store_date}@{${def:date}}
 menor o igual que].' date1='sec_fecha_registro_ini' date2='sec_fecha_registro_fin' id='sec_fecha_registro_ini' />
	<parameter id='sec_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
