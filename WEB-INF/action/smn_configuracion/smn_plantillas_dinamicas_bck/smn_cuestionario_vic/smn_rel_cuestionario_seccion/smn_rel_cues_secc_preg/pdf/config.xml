<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnrelcuesseccpreg.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lbl:b_smn_rel_cues_secc_preg}</pdf-title>
	<pdf-logo url='/images/logo-simone-pdf.png' scale='100' />
	<pdf-file-name>${lbl:b_smn_rel_cues_secc_preg}.pdf</pdf-file-name>

	<pdf-report 
		pageSize="letter" 
		rotate="false"
		footer="false" 
		header="true">
		<record recordset='query-smnrelcuesseccpreg.sql' width='100' spacingBefore='40'>
			<col name='smn_rel_cuestionario_seccion_id' title='${lbl:b_smn_rel_cuestionario_seccion_id}' width='33' align='left' format='######' />
			<col name='prg_codigo_pl0' title='${lbl:b_smn_pregunta_id}' width='33' align='left' />
			<col name='csp_secuencia' title='${lbl:b_csp_secuencia}' width='33' align='left' format='######' />

		</record>

	</pdf-report>
</config>
