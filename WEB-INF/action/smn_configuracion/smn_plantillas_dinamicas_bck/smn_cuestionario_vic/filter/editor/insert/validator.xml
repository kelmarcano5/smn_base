<?xml version='1.0' encoding='ISO-8859-1'?>
<validator onerror='/action/error/validation/ajax'>
	<parameter id='cue_codigo' type='varchar' required='true' label='${lbl:b_cue_codigo}' maxlen='48'/>
	<parameter id='cue_descripcion' type='varchar' required='true' label='${lbl:b_cue_descripcion}' maxlen='100'/>
	<parameter id='smn_modulo_rf' type='integer' required='true' label='${lbl:b_smn_modulo_rf}' maxlen='10'/>
	<parameter id='smn_esquema_rf' type='integer' required='true' label='${lbl:b_smn_esquema_rf}' maxlen='10'/>
	<parameter id='smn_campo_rf' type='integer' required='true' label='${lbl:b_smn_campo_rf}' maxlen='10'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='cue_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='cue_descripcion'/>



</validator>
