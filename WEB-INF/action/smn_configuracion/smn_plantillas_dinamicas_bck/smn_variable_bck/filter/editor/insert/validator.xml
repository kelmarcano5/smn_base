	<parameter id='var_codigo' type='varchar' required='true' label='${lbl:b_var_codigo}' maxlen='16'/>
	<parameter id='var_descripción' type='varchar' required='true' label='${lbl:b_var_descripcion}' maxlen='100'/>

	<custom-validator
