<?xml version='1.0' encoding='ISO-8859-1'?>
<validator onerror='/action/error/validation/ajax'>
	<parameter id='smn_cuestionario_id' type='integer' required='true' label='${lbl:b_smn_cuestionario_id}' maxlen='10'/>
	<parameter id='sec_codigo' type='varchar' required='true' label='${lbl:b_sec_codigo}' maxlen='16'/>
	<parameter id='sec_descripcion' type='varchar' required='true' label='${lbl:b_sec_descripcion}' maxlen='100'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='sec_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='sec_descripcion'/>



</validator>
