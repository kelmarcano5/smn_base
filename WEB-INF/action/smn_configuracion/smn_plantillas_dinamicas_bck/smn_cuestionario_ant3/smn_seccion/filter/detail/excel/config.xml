<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="4" cellStep="0" name="${lbl:b_smn_seccion}"/>
	
	<excel 
		recordset='query.sql' 
		sheetname='${lbl:b_smn_seccion}' 
		filename='${lbl:b_smn_seccion}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='cue_codigo_pl0' label='${lbl:b_smn_cuestionario_id}' />
		<col id='sec_codigo' label='${lbl:b_sec_codigo}' />
		<col id='sec_descripcion' label='${lbl:b_sec_descripcion}' />
		<col id='sec_secuencia' label='${lbl:b_sec_secuencia}' />
		<col id='sec_estatus_combo' label='${lbl:b_status}' />
		<col id='cue_idioma' label='${lbl:s_profile_lang}' />
		<col id='cue_usuario' label='${lbl:s_profile_lang}' />
		<col id='cue_fecha_registro' label='${lbl:b_store_date}' />
		<col id='cue_hora' label='${lbl:s_loginhist_col2}' />

	</excel>

</config>
