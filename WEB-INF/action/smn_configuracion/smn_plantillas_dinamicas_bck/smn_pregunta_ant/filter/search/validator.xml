<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_tipo_variable_id_name' type='varchar' required='false' label='${lbl:b_smn_tipo_variable_id}'/>
	<parameter id='smn_tipo_variable_id' type='integer' required='false' label='${lbl:b_smn_tipo_variable_id}' maxlen='10'/>
	<parameter id='prg_codigo' type='varchar' required='false' label='${lbl:b_prg_codigo}' maxlen='16'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='prg_codigo' rule='like' />
	<parameter id='prg_descripcion' type='varchar' required='false' label='${lbl:b_prg_descripcion}' maxlen='100'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='prg_descripcion' rule='like' />
	<parameter id='prg_tipo_respuesta' type='varchar' required='false' label='${lbl:b_prg_tipo_respuesta}' maxlen='2'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='prg_tipo_respuesta' rule='like' />
	<parameter id='cue_idioma' type='varchar' required='false' label='${lbl:s_profile_lang}' maxlen='2'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='cue_idioma' rule='like' />
	<parameter id='cue_fecha_registro_ini' type='date' required='false' label='${lbl:s_loginhist_col2}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:s_loginhist_col2}@{${def:time}}
 mayor o igual que] no puede ser mayor que [${lbl:s_loginhist_col2}@{${def:time}}
 menor o igual que].' date1='cue_fecha_registro_ini' date2='cue_fecha_registro_fin' id='cue_fecha_registro_ini' />
	<parameter id='cue_fecha_registro_fin' type='date' required='false' label='${lbl:s_loginhist_col2}' maxlen='10'/>
</validator>
