<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smntipovariable.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="3" cellStep="0" name="${lbl:b_smn_tipo_variable}"/>
	
	<excel 
		recordset='query-smntipovariable.sql' 
		sheetname='${lbl:b_smn_tipo_variable}' 
		filename='${lbl:b_smn_tipo_variable}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='tva_codigo' label='${lbl:b_tva_codigo}' />
		<col id='tva_descripcion' label='${lbl:b_tva_descripcion}' />
		<col id='tva_estatus_combo' label='${lbl:b_status}' />
		<col id='tva_idioma' label='${lbl:s_profile_lang}' />
		<col id='tva_usuario' label='${lbl:b_users}' />
		<col id='tva_fecha_registro' label='${lbl:b_store_date}' />
		<col id='tva_hora' label='${lbl:s_loginhist_col2}' />

	</excel>

</config>
