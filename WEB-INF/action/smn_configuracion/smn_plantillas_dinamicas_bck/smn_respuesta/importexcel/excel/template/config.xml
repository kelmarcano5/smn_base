<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnrespuesta.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_smn_respuesta}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_smn_respuesta}">
				<col id='cue_codigo_pl0' label='${lbl:b_smn_cuestionario_id}' />
		<col id='sec_codigo_pl1' label='${lbl:b_smn_seccion_id}' />
		<col id='prg_codigo_pl2' label='${lbl:b_smn_pregunta_id}' />
		<col id='tva_codigo_pl3' label='${lbl:b_smn_tipo_variable_id}' />
		<col id='smn_tipo_respuesta_id_combo' label='${lbl:b_smn_tipo_respuesta_id}' />
		<col id='smn_variable_id' label='${lbl:b_smn_variable}' />
		<col id='res_texto_respuesta' label='${lbl:b_res_texto_respuesta}' />
		<col id='cue_idioma' label='${lbl:s_profile_lang}' />
		<col id='cue_usuario' label='${lbl:b_users}' />
		<col id='cue_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
	
	</excel>

</config>
