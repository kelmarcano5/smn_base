<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnrelprocesorespuesta.sql" source="session" scope="transaction" />
		
	</transaction>

	<output>
		<classname>dinamica.GenericExcelOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_smn_rel_proceso_respuesta}.xls' >
		
	<sheet recordset="query-table_stuck}.sql" sheetname="${lbl:b_smn_rel_proceso_respuesta}">
				<col id='cue_codigo_pl0' label='${lbl:b_smn_cuestionario_id}' />
		<col id='smn_modulo_rf_combo' label='${lbl:b_smn_modulo_rf}' />
		<col id='smn_esquema_rf_combo' label='${lbl:b_smn_esquema_rf}' />
		<col id='smn_campo_rf_combo' label='${lbl:b_smn_campo_rf}' />
		<col id='smn_respuesta_id' label='${lbl:b_smn_respuesta_id}' />
		<col id='cue_idioma' label='${lbl:s_profile_lang}' />
		<col id='cue_usuario' label='${lbl:b_users}' />
		<col id='cue_fecha_registro' label='${lbl:b_store_date}' />

	</sheet>
	
	</excel>

</config>
