<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnrelprocesorespuesta.sql" source="session" scope="transaction" />
		<recordset id="filter.params" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lbl:b_smn_rel_proceso_respuesta}</pdf-title>
	<pdf-logo url='/images/logo-simone-pdf.png' scale='100' align="left" />
	<pdf-file-name>${lbl:b_smn_rel_proceso_respuesta}.pdf</pdf-file-name>

	<pdf-report pageSize="letter" rotate="false" footer="true" header="true">
		<record recordset='filter.params' title='${lbl:b_filter_find}' noFilterMsg='-- ${lbl:b_all_message} --' width='55' spacingBefore='15'>
			<col name='smn_cuestionario_id_name' title='${lbl:b_smn_cuestionario_id}'  align='left' />
			<col name='smn_modulo_rf' title='${lbl:b_smn_modulo_rf}'  format='######' align='left' />
			<col name='smn_esquema_rf' title='${lbl:b_smn_esquema_rf}'  format='######' align='left' />
			<col name='smn_campo_rf' title='${lbl:b_smn_campo_rf}'  format='######' align='left' />
			<col name='smn_respuesta_id' title='${lbl:b_smn_respuesta_id}'  format='######' align='left' />
			<col name='cue_fecha_registro_ini' title='${lbl:b_store_date}'  format='dd-MM-yyyy' align='left' />
			<col name='cue_fecha_registro_fin' title='${lbl:b_store_date}'  format='dd-MM-yyyy' align='left' />

		</record>
		<record recordset='query-smnrelprocesorespuesta.sql' width='100' spacingBefore='10'>
			<col name='cue_codigo_pl0' title='${lbl:b_smn_cuestionario_id}' width='11' align='left' />
			<col name='smn_modulo_rf_combo' title='${lbl:b_smn_modulo_rf}' width='11' align='left' format='######' />
			<col name='smn_esquema_rf_combo' title='${lbl:b_smn_esquema_rf}' width='11' align='left' format='######' />
			<col name='smn_campo_rf_combo' title='${lbl:b_smn_campo_rf}' width='11' align='left' format='######' />
			<col name='smn_respuesta_id' title='${lbl:b_smn_respuesta_id}' width='11' align='left' format='######' />
			<col name='cue_idioma' title='${lbl:s_profile_lang}' width='11' align='left' />
			<col name='cue_usuario' title='${lbl:b_users}' width='11' align='left' />
			<col name='cue_fecha_registro' title='${lbl:b_store_date}' width='11' align='left' format='dd-MM-yyyy' />
			<col name='cue_hora' title='${lbl:s_loginhist_col2}' width='11' align='left' />

		</record>

	</pdf-report>
</config>
