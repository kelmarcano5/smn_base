<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />

	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lbl:b_smn_rel_proceso_respuesta}</pdf-title>
	<pdf-logo url='/images/logo-simone-pdf.png' scale='100' />
	<pdf-file-name>${lbl:b_smn_rel_proceso_respuesta}.pdf</pdf-file-name>

	<pdf-report pageSize="letter" rotate="false" footer="true" header="true">
		<record recordset='query.sql' title='${lbl:b_smn_rel_proceso_respuesta}' width='60' spacingBefore='40'>
			<col name='cue_codigo_pl0' title='${lbl:b_smn_cuestionario_id}' align='left' />
			<col name='smn_modulo_rf_combo' title='${lbl:b_smn_modulo_rf}' align='left' format='######' />
			<col name='smn_esquema_rf_combo' title='${lbl:b_smn_esquema_rf}' align='left' format='######' />
			<col name='smn_campo_rf_combo' title='${lbl:b_smn_campo_rf}' align='left' format='######' />
			<col name='smn_respuesta_id' title='${lbl:b_smn_respuesta_id}' align='left' format='######' />
			<col name='cue_idioma' title='${lbl:s_profile_lang}' align='left' />
			<col name='cue_usuario' title='${lbl:b_users}' align='left' />
			<col name='cue_fecha_registro' title='${lbl:b_store_date}' align='left' format='dd-MM-yyyy' />
			<col name='cue_hora' title='${lbl:s_loginhist_col2}' align='left' />
		</record>


	</pdf-report>
</config>
