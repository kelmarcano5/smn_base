<?xml version='1.0' encoding='ISO-8859-1'?>
<validator onerror='/action/error/validation/ajax'>
	<parameter id='naa_codigo' type='varchar' required='true' label='${lbl:b_code}' maxlen='16'/>
	<parameter id='naa_nombre' type='varchar' required='true' label='${lbl:b_name}' maxlen='48'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='naa_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarNombre.sql'
			id='naa_nombre'/>


</validator>
