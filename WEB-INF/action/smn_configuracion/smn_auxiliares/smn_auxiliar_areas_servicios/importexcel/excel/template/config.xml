<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="false">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnauxiliarareasservicios-template.sql" source="sql" scope="transaction" />
		<recordset id='query-smnvauxiliar.sql' source='sql' scope='transaction' />
		<recordset id='query-smnauxiliarsucursales.sql' source='sql' scope='transaction' />
		<recordset id='query-smnauxiliarunidadesnegocios.sql' source='sql' scope='transaction' />
		<recordset id="query-smnvauxiliartiposservicios.sql" source="sql" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.repgen.smn_configuracion.smn_auxiliares.smn_auxiliar_areas_servicios.ExcelReportOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_outsourced_services_area}.xls'      
		header-background='true' >
		
	<sheet recordset="query-smnauxiliarareasservicios-template.sql" sheetname="${lbl:b_outsourced_services_area}">
		
		<col id='smn_auxiliar_tipos_servicios' label='${lbl:b_outsourced_type_of_service}' />
		<col id='tse_codigo' label='${lbl:b_code}' />
		<col id='tse_descripcion' label='${lbl:b_description}' />
		<col id='tse_auxiliar' label='${lbl:b_auxiliary}' />
		<col id='tse_auxiliar_sucursal' label='${lbl:b_outsourced_subsidiary}' />
		<col id='tse_auxiliar_unidad_negocio' label='${lbl:b_outsourced_unit_business}' />
		
		<col id='smn_auxiliar_tipos_servicios_reference' label='${lbl:b_outsourced_type_of_service}' />
		<col id='tse_auxiliar_reference' label='${lbl:b_auxiliary}' />
		<col id='tse_auxiliar_sucursal_reference' label='${lbl:b_outsourced_subsidiary}' />
		<col id='tse_auxiliar_unidad_negocio_reference' label='${lbl:b_outsourced_unit_business}' />


	</sheet>
	
	<sheet recordset='query-smnvauxiliartiposservicios.sql' sheetname='${lbl:b_outsourced_type_of_service}'>
		<col id='tts_descripcion' label='${lbl:b_outsourced_type_of_service}' />
	</sheet>
	
	<sheet recordset='query-smnvauxiliar.sql' sheetname='${lbl:b_auxiliary}'>
		<col id='smn_item' label='${lbl:b_auxiliary}' />
	</sheet>

	<sheet recordset='query-smnauxiliarsucursales.sql' sheetname='${lbl:b_outsourced_subsidiary}'>
		<col id='trs_nombre' label='${lbl:b_name}' />
	</sheet>

	<sheet recordset='query-smnauxiliarunidadesnegocios.sql' sheetname='${lbl:b_outsourced_unit_business}'>
		<col id='tun_nombre' label='${lbl:b_name}' />
	</sheet>


	</excel>

</config>
