<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_auxiliar_tipos_servicios_id_name' type='varchar' required='false' label='${lbl:b_outsourced_type_of_service}'/>
	<parameter id='smn_auxiliar_tipos_servicios_id' type='integer' required='false' label='${lbl:b_outsourced_type_of_service}' maxlen='10'/>
	<parameter id='tse_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='tse_codigo' rule='contains' />
	<parameter id='tse_descripcion' type='varchar' required='false' label='${lbl:b_description}' maxlen='48'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='tse_descripcion' rule='contains' />
	<parameter id='tse_auxiliar' type='integer' required='false' label='${lbl:b_auxiliary}' maxlen='10'/>
	<parameter id='tse_auxiliar_sucursal' type='integer' required='false' label='${lbl:b_outsourced_subsidiary}' maxlen='10'/>
	<parameter id='tse_auxiliar_unidad_negocio' type='integer' required='false' label='${lbl:b_outsourced_unit_business}' maxlen='10'/>
	<parameter id='tse_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date_grater_than}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date_grater_than} mayor o igual que] no puede ser mayor que [${lbl:b_store_date_less_than} menor o igual que].' date1='tse_fecha_registro_ini' date2='tse_fecha_registro_fin' id='tse_fecha_registro_ini' />
	<parameter id='tse_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date_less_than}' maxlen='10'/>
</validator>
