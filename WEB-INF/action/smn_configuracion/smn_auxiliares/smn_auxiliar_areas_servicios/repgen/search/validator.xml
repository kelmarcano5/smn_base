<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='repgen.filter' onerror='/action/error/validation/ajax'>

	<parameter id='fdesde' type='date' required='false' label='Fecha desde'/>
	<parameter id='fhasta' type='date' required='false' label='Fecha hasta'/>
	
	<parameter id='smn_auxiliar_tipos_servicios_id_name' type='varchar' required='false' label='${lbl:b_outsourced_type_of_service}'/>
	<parameter id='smn_auxiliar_tipos_servicios_id' type='integer' required='false' label='${lbl:b_outsourced_type_of_service}' maxlen='10'/>
	<parameter id='tse_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<parameter id='tse_descripcion' type='varchar' required='false' label='${lbl:b_description}' maxlen='48'/>
	<parameter id='tse_auxiliar' type='integer' required='false' label='${lbl:b_auxiliary}' maxlen='10'/>
	<parameter id='tse_auxiliar_sucursal' type='integer' required='false' label='${lbl:b_outsourced_subsidiary}' maxlen='10'/>
	<parameter id='tse_auxiliar_unidad_negocio' type='integer' required='false' label='${lbl:b_outsourced_unit_business}' maxlen='10'/>

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='smn_auxiliar_tipos_servicios_id_name'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
		parameter='tse_codigo'
		rule='contains' />

	<custom-validator
		classname='dinamica.validators.SQLPatternTransformer'
parameter='tse_descripcion'
		rule='contains' />


	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='tse_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='tse_descripcion'/>



	<custom-validator
			classname='dinamica.validators.ArrayNotEmptyValidator'
			on-error-label='Seleccione al menos un campo para la generación del reporte.'
			parameter-name='field_id' 
			id='field_id' />

	<custom-validator 
			classname='dinamica.validators.ArrayToRecordset'
			paramName='field_id' 
			recordsetId='fields' 
			colName='field_id' 
			colType='integer' />
				
</validator>
