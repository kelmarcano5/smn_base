<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnauxiliarareasservicios.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="3" cellStep="0" name="${lbl:b_service_areas}"/>
	
	<excel 
		recordset='query-smnauxiliarareasservicios.sql' 
		sheetname='${lbl:b_service_areas}' 
		filename='${lbl:b_service_areas}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='tts_codigo_pl0' label='${lbl:b_outsourced_type_of_service}' />
		<col id='tse_codigo' label='${lbl:b_code}' />
		<col id='tse_descripcion' label='${lbl:b_description}' />
		<col id='smn_item' label='${lbl:b_auxiliary}' />
		<col id='smn_item_sucursal' label='${lbl:b_outsourced_subsidiary}' />
		<col id='smn_item_unidad_negocio' label='${lbl:b_outsourced_unit_business}' />
		<col id='tse_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
