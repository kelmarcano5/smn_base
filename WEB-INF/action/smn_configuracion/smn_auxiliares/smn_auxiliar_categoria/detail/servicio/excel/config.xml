<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
		<recordset id='detail-smnauxiliarprestadorservicio.sql' source='session' scope='transaction' />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="0" cellPosX="7" cellStep="0" name="${lbl:b_assistant_service_provider}"/>
	
	<excel 
		recordset='query.sql' 
		sheetname='${lbl:b_assistant_service_provider}' 
		filename='${lbl:b_assistant_service_provider}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='naa_nombre' label='${lbl:b_auxiliary_class}' />
		<col id='aca_tipo_persona_combo' label='${lbl:b_kind_of_person}' />
		<col id='ent_descripcion_corta' label='${lbl:b_entity}'  />
		<col id='suc_nombre' label='${lbl:b_subsidiary}' />
		<col id='uen_nombre' label='${lbl:b_business_unit}' />
		<col id='ase_descripcion' label='${lbl:b_service_area}' />
		<col id='uns_descripcion' label='${lbl:b_service_unit}' />
		<col id='aca_fecha_registro' label='${lbl:b_store_date}' />
		
		<!--opcional-->
		<sheet recordset="detail-smnauxiliarprestadorservicio.sql" sheetname="${lbl:b_assistant_service_provider_detail}">
			<col id='aps_codigo' label='${lbl:b_code}' />
			<col id='aps_descripcion' label='${lbl:b_description}' />
			<col id='aps_beneficiario' label='${lbl:b_beneficiary}' />
			<col id='tdo_descripcion' label='${lbl:b_official_document_type}'  />
			<col id='aps_num_doc_oficial' label='${lbl:b_official_document_number}' />
			<col id='aps_clasif_abc_combo' label='${lbl:b_abc_clasification}' />
			<col id='aps_rif' label='${lbl:b_tax_id}' />
			<col id='dir_descripcion' label='${lbl:b_business_address}'  />
			<col id='acb_num_cuenta' label='${lbl:b_auxiliary_bank_account}'  />
			<col id='acn_telefono_fijo' label='${lbl:b_auxiliary_contact}'  />
			<col id='aps_persona_contacto' label='${lbl:b_contact_person}' />
			<col id='aps_representante_legal' label='${lbl:b_legal_representative}' />
			<col id='aps_benef_pago' label='${lbl:b_beneficiary_payments}' />
			<col id='cop_descripcion' label='${lbl:b_condition_payments}'  />
			<col id='aps_observacion' label='${lbl:b_observation}' />
			<col id='aps_fecha_registro' label='${lbl:b_store_date}' />
 		</sheet>
 		
	</excel>

</config>
