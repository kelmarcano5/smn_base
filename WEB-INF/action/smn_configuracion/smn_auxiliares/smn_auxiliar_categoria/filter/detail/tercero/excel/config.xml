<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
		<recordset id='detail-smnauxiliartercero.sql' source='session' scope='transaction' />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="0" cellPosX="11" cellStep="0" name="${lbl:b_intermediary_assistant}"/>
	
	<excel 
		recordset='query.sql' 
		sheetname='${lbl:b_intermediary_assistant}' 
		filename='${lbl:b_intermediary_assistant}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='naa_nombre' label='${lbl:b_auxiliary_class}' />
		<col id='aca_tipo_persona_combo' label='${lbl:b_kind_of_person}' />
		<col id='ent_descripcion_corta' label='${lbl:b_entity}'  />
		<col id='suc_nombre' label='${lbl:b_subsidiary}' />
		<col id='uen_nombre' label='${lbl:b_business_unit}' />
		<col id='ase_descripcion' label='${lbl:b_service_area}' />
		<col id='uns_descripcion' label='${lbl:b_service_unit}' />
		<col id='aca_fecha_registro' label='${lbl:b_store_date}' />
		
		<!--opcional-->
		<sheet recordset="detail-smnauxiliartercero.sql" sheetname="${lbl:b_intermediary_assistant_detail}">
			<col id='atr_codigo' label='${lbl:b_code}' />
			<col id='atr_descripcion' label='${lbl:b_description}' />
			<col id='cmn_nombre' label='${lbl:b_community}' />
			<col id='tcp_nombre' label='${lbl:b_outsourced_corporation}' />
			<col id='tun_nombre' label='${lbl:b_outsourced_unit_business}' />
			<col id='atr_maneja_sucursal_combo' label='${lbl:b_has_subsidiary}' />
			<col id='atr_razon_social' label='${lbl:b_business_name}' />
			<col id='atr_denom_comercial' label='${lbl:b_designation_commercial}' />
			<col id='atr_beneficiario' label='${lbl:b_beneficiary}' />
			<col id='tdo_descripcion' label='${lbl:b_official_document_type}' />
			<col id='atr_num_doc_oficial' label='${lbl:b_official_document_number}' />
			<col id='atr_clasif_abc_combo' label='${lbl:b_abc_clasification}' />
			<col id='atr_rif' label='${lbl:b_tax_id}' />
			<col id='dir_descripcion' label='${lbl:b_business_address}' />
			<col id='acb_num_cuenta' label='${lbl:b_auxiliary_bank_account}' />
			<col id='acn_telefono_fijo' label='${lbl:b_auxiliary_contact}' />
			<col id='atr_persona_contacto' label='${lbl:b_contact_person}' />
			<col id='atr_representante_legal' label='${lbl:b_legal_representative}' />
			<col id='atr_benef_pago' label='${lbl:b_beneficiary_payments}' />
			<col id='cop_descripcion' label='${lbl:b_condition_payments}' />
			<col id='atr_tipo_facturac_combo' label='${lbl:b_billing_type}' />
			<col id='cfn_descripcion' label='${lbl:b_financial_condition}' />
			<col id='atr_observacion' label='${lbl:b_observation}' />
			<col id='atr_fecha_registro' label='${lbl:b_store_date}' />
 		</sheet>
 		
	</excel>

</config>
