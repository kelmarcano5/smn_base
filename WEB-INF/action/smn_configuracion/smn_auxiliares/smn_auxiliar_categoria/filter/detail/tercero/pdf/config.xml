<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="false">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
		<recordset id='detail-smnauxiliartercero.sql' source='session' scope='transaction' />
		
	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lbl:b_auxiliary}</pdf-title>
	<pdf-logo url='/images/logo-simone-pdf.png' scale='100' />
	<pdf-file-name>${lbl:b_auxiliary}.pdf</pdf-file-name>

	<pdf-report pageSize="letter" rotate="false" footer="true" header="true">
		<record recordset='query.sql' title='${lbl:b_auxiliary}' width='60' spacingBefore='40'>
			<col name='naa_nombre' title='${lbl:b_auxiliary_class}' align='left' />
			<col name='aca_tipo_persona_combo' title='${lbl:b_kind_of_person}' align='left' />
			<col name='ent_descripcion_corta' title='${lbl:b_entity}' align='left'  />
			<col name='suc_nombre' title='${lbl:b_subsidiary}' align='left' />
			<col name='uen_nombre' title='${lbl:b_business_unit}' align='left' />
			<col name='ase_descripcion' title='${lbl:b_service_area}' align='left' />
			<col name='uns_descripcion' title='${lbl:b_service_unit}' align='left' />
			<col name='aca_fecha_registro' title='${lbl:b_store_date}' align='left' format='dd-MM-yyyy' />
		</record>
		<record recordset='detail-smnauxiliartercero.sql' width='100' spacingBefore='40'>
			<col name='atr_codigo' title='${lbl:b_code}' align='left' />
			<col name='atr_descripcion' title='${lbl:b_description}' align='left' />
			<col name='cmn_nombre' title='${lbl:b_community}' align='left' format='######' />
			<col name='tcp_nombre' title='${lbl:b_outsourced_corporation}' align='left' format='######' />
			<col name='tun_nombre' title='${lbl:b_outsourced_unit_business}' align='left' format='######' />
			<col name='atr_maneja_sucursal_combo' title='${lbl:b_has_subsidiary}' align='left' />
			<col name='atr_razon_social' title='${lbl:b_business_name}' align='left' />
			<col name='atr_denom_comercial' title='${lbl:b_designation_commercial}' align='left' />
			<col name='atr_beneficiario' title='${lbl:b_beneficiary}' align='left' />
			<col name='tdo_descripcion' title='${lbl:b_official_document_type}' align='left' format='######' />
			<col name='atr_num_doc_oficial' title='${lbl:b_official_document_number}' align='left' />
			<col name='atr_clasif_abc_combo' title='${lbl:b_abc_clasification}' align='left' />
			<col name='atr_rif' title='${lbl:b_tax_id}' align='left' />
			<col name='dir_descripcion' title='${lbl:b_business_address}' align='left' format='######' />
			<col name='acb_num_cuenta' title='${lbl:b_auxiliary_bank_account}' align='left' format='######' />
			<col name='acn_telefono_fijo' title='${lbl:b_auxiliary_contact}' align='left' format='######' />
			<col name='atr_persona_contacto' title='${lbl:b_contact_person}' align='left' />
			<col name='atr_representante_legal' title='${lbl:b_legal_representative}' align='left' />
			<col name='atr_benef_pago' title='${lbl:b_beneficiary_payments}' align='left' />
			<col name='cop_descripcion' title='${lbl:b_condition_payments}' align='left' format='######' />
			<col name='atr_tipo_facturac_combo' title='${lbl:b_billing_type}' align='left' />
			<col name='cfn_descripcion' title='${lbl:b_financial_condition}' align='left' format='######' />
			<col name='atr_observacion' title='${lbl:b_observation}' align='left' />
			<col name='atr_fecha_registro' title='${lbl:b_store_date}' align='left' format='dd-MM-yyyy' />
		</record>

	</pdf-report>
</config>
