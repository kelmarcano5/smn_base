<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
		<recordset id='detail-smnauxiliarpersonanatural.sql' source='session' scope='transaction' />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="0" cellPosX="2" cellStep="0" name="${lbl:b_person_assistant}"/>
	
	<excel 
		recordset='query.sql' 
		sheetname='${lbl:b_person_assistant}' 
		filename='${lbl:b_person_assistant}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='naa_nombre' label='${lbl:b_auxiliary_class}' />
		<col id='aca_tipo_persona_combo' label='${lbl:b_kind_of_person}' />
		<col id='ent_descripcion_corta' label='${lbl:b_entity}'  />
		<col id='suc_nombre' label='${lbl:b_subsidiary}' />
		<col id='uen_nombre' label='${lbl:b_business_unit}' />
		<col id='ase_descripcion' label='${lbl:b_service_area}' />
		<col id='uns_descripcion' label='${lbl:b_service_unit}' />
		<col id='aca_fecha_registro' label='${lbl:b_store_date}' />
		
		<!--opcional-->
		<sheet recordset="detail-smnauxiliarpersonanatural.sql" sheetname="${lbl:b_person_assistant_detail}">
			<col id='ant_codigo' label='${lbl:b_code}' />
			<col id='ant_descripcion' label='${lbl:b_description}' />
			<col id='ant_nombres' label='${lbl:s_profile_fname}' />
			<col id='ant_apellidos' label='${lbl:s_profile_lname}' />
			<col id='tdo_descripcion' label='${lbl:b_official_document_type}'  />
			<col id='ant_num_doc_oficial' label='${lbl:b_official_document_number}' />
			<col id='ant_fecha_nac' label='${lbl:b_birth_date}' />
			<col id='ant_lugar_nac' label='${lbl:b_birth_place}' />
			<col id='ant_rif' label='${lbl:b_tax_id}' />
			<col id='pai_gentilicio' label='${lbl:b_citizenship}'  />
			<col id='ant_nacionalizado_combo' label='${lbl:b_nationalized}' />
			<col id='ant_sexo_combo' label='${lbl:b_sex}' />
			<col id='ant_estado_civil_combo' label='${lbl:b_civil_status}' />
			<col id='dir_descripcion' label='${lbl:b_business_address}'  />
			<col id='acb_num_cuenta' label='${lbl:b_auxiliary_bank_account}'  />
			<col id='acn_telefono_fijo' label='${lbl:b_auxiliary_contact}'  />
			<col id='ant_num_gaceta' label='${lbl:b_nationalization_gazette}'  />
			<col id='ant_fecha_gaceta_nac' label='${lbl:b_date_of_naturalization}' />
			<col id='cfn_descripcion' label='${lbl:b_financial_condition}'  />
			<col id='ant_persona_contacto' label='${lbl:b_contact_person}' />
			<col id='ant_observacion' label='${lbl:b_observation}' />
			<col id='ant_fecha_registro' label='${lbl:b_store_date}' />
 		</sheet>

	</excel>

</config>
