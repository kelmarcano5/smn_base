	<parameter id='smn_tipo_contactos_rf' type='integer' required='true' label='${lbl:b_type_contact}' maxlen='10'/>
	<parameter id='auc_descripcion' type='varchar' required='true' label='${lbl:b_description}' maxlen='24'/>
