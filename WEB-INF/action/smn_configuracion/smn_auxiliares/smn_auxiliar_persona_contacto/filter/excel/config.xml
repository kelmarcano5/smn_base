<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnauxiliarpersonacontacto.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="3" cellStep="0" name="${lbl:b_person_contact}"/>
	
	<excel 
		recordset='query-smnauxiliarpersonacontacto.sql' 
		sheetname='${lbl:b_person_contact}' 
		filename='${lbl:b_person_contact}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='smn_clase_auxiliar_rf' label='${lbl:b_class_auxiliary}' />
		<col id='smn_auxiliar_rf' label='${lbl:b_auxiliary}' />
		<col id='apc_apellidos' label='${lbl:s_profile_lname}' />
		<col id='apc_nombres' label='${lbl:s_profile_fname}' />
		<col id='apc_telefono_contacto' label='${lbl:b_phone}' />
		<col id='apc_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
