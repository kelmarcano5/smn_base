<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="false">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnauxiliarunidadesnegocios-template.sql" source="sql" scope="transaction" />
		<recordset id='query-smndireccion.sql' source='sql' scope='transaction' />	
	</transaction>

	<output>
		<classname>domain.repgen.smn_configuracion.smn_auxiliares.smn_auxiliar_unidades_negocios.ExcelReportOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_outsourced_units_business}.xls'      
		header-background='true' >
		
	<sheet recordset="query-smnauxiliarunidadesnegocios-template.sql" sheetname="${lbl:b_outsourced_units_business}">
		
		<col id='tun_codigo' label='${lbl:b_code}' />
		<col id='tun_nombre' label='${lbl:b_name}' />
		<col id='tun_responsable' label='${lbl:b_responsible}' />
		<col id='tun_direccion' label='${lbl:b_address}' />
		<col id='tun_telefono_fijo' label='${lbl:b_fixed_phone}' />
		<col id='tun_telefono_movil' label='${lbl:b_mobile_phone}' />
		<col id='tun_email' label='${lbl:s_profile_email}' />
		
		<col id='tun_direccion_reference' label='${lbl:b_address}' />

	</sheet>
	
	<sheet recordset='query-smndireccion.sql' sheetname='${lbl:b_address}'>
		<col id='dir_descripcion' label='${lbl:b_description}' />
	</sheet>


	</excel>

</config>
