<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="4" cellStep="0" name="${lbl:b_outsourced_units_business}"/>
	
	<excel 
		recordset='query.sql' 
		sheetname='${lbl:b_outsourced_units_business}' 
		filename='${lbl:b_outsourced_units_business}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='tun_codigo' label='${lbl:b_code}' />
		<col id='tun_nombre' label='${lbl:b_name}' />
		<col id='tun_responsable' label='${lbl:b_responsible}' />
		<col id='dir_descripcion' label='${lbl:b_address}' />
		<col id='tun_telefono_fijo' label='${lbl:b_fixed_phone}' />
		<col id='tun_telefono_movil' label='${lbl:b_mobile_phone}' />
		<col id='tun_email' label='${lbl:s_profile_email}' />
		<col id='tun_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
