<?xml version='1.0' encoding='ISO-8859-1'?>
<validator maxErrors='20'>
	<parameter id='smn_naturaleza_auxiliar_rf' type='integer' required='true' maxlen='10'/>
	<parameter id='smn_clase_auxiliar_rf' type='integer' required='true' maxlen='10'/>
	<parameter id='aux_tipo_persona' type='varchar' required='true' maxlen='2'/>
	<parameter id='aux_codigo' type='varchar' required='false' maxlen='16'/>
	<parameter id='aux_descripcion' type='varchar' required='false' maxlen='64'/>
	<parameter id='aux_apellidos' type='varchar' required='false' maxlen='64'/>
	<parameter id='aux_nombres' type='varchar' required='false' maxlen='64'/>
	<parameter id='aux_num_coleg_medicos' type='varchar' required='false' maxlen='16'/>


	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarCodigo.sql'
			id='aux_codigo'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarDescripcion.sql'
			id='aux_descripcion'/>

	<custom-validator
		classname='dinamica.validators.DuplicatedKeyValidator'
			on-error-label='${lbl:b_record_not_duplicated}.'
			sql='buscarNombre.sql'
			id='aux_nombres'/>



</validator>
