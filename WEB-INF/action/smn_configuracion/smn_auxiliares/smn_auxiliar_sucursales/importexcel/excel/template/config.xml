<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="false">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnauxiliarsucursales-template.sql" source="sql" scope="transaction" />
		<recordset id='query-smnauxiliarunidadesnegocios.sql' source='sql' scope='transaction' />
	<recordset id='query-smndireccion.sql' source='sql' scope='transaction' />
	
	</transaction>

	<output>
		<classname>domain.repgen.smn_configuracion.smn_auxiliares.smn_auxiliar_sucursales.ExcelReportOutput</classname>
	</output>
		
	<excel 
		filename='${lbl:b_outsourced_subsidiaries}.xls'      
		header-background='true' >
		
	<sheet recordset="query-smnauxiliarsucursales-template.sql" sheetname="${lbl:b_outsourced_subsidiaries}">
		
		<col id='trs_codigo' label='${lbl:b_code}' />
		<col id='trs_nombre' label='${lbl:b_name}' />
		<col id='trs_auxiliar_unidad_negocio' label='${lbl:b_outsourced_unit_business}' />
		<col id='trs_responsable' label='${lbl:b_responsible}' />
		<col id='trs_direccion' label='${lbl:b_address}' />
		<col id='trs_telefono_fijo' label='${lbl:b_fixed_phone}' />
		<col id='trs_telefono_movil' label='${lbl:b_mobile_phone}' />
		<col id='trs_email' label='${lbl:s_profile_email}' />
		
		<col id='trs_auxiliar_unidad_negocio_reference' label='${lbl:b_outsourced_unit_business}' />
		<col id='trs_direccion_reference' label='${lbl:b_address}' />

	</sheet>
	
	<sheet recordset='query-smnauxiliarunidadesnegocios.sql' sheetname='${lbl:b_outsourced_unit_business}'>
		<col id='tun_nombre' label='${lbl:b_name}' />
	</sheet>

	<sheet recordset='query-smndireccion.sql' sheetname='${lbl:b_address}'>
		<col id='dir_descripcion' label='${lbl:b_description}' />
	</sheet>


	</excel>

</config>
