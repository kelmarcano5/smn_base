<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Generar reporte generico PDF
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query.sql" source="session" scope="transaction" />

	</transaction>

	<output>
		<classname>dinamica.PDFGenericReport</classname>
	</output>
	
	<pdf-title>${lbl:<pdf-title>${lblb_outsourced_subsidiaries}</pdf-title>
	<pdf-logo url='/images/logo-simone-pdf.png' scale='100' />
	<pdf-file-name>${lbl:<pdf-title>${lblb_outsourced_subsidiaries}.pdf</pdf-file-name>

	<pdf-report pageSize="letter" rotate="false" footer="true" header="true">
		<record recordset='query.sql' title='${lblb_outsourced_subsidiaries}' width='60' spacingBefore='40'>
			<col name='trs_codigo' title='${lbl:b_code}' align='left' />
			<col name='trs_nombre' title='${lbl:b_name}' align='left' />
			<col name='tun_nombre' title='${lbl:b_outsourced_unit_business}' align='left'  />
			<col name='trs_responsable' title='${lbl:b_responsible}' align='left' />
			<col name='dir_descripcion' title='${lbl:b_address}' align='left'  />
			<col name='trs_telefono_fijo' title='${lbl:b_fixed_phone}' align='left' />
			<col name='trs_telefono_movil' title='${lbl:b_mobile_phone}' align='left' />
			<col name='trs_email' title='${lbl:s_profile_email}' align='left' />
			<col name='trs_fecha_registro' title='${lbl:b_store_date}' align='left' format='dd-MM-yyyy' />
		</record>



	</pdf-report>
</config>
