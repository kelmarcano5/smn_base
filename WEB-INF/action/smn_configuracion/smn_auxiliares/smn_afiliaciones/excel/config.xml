<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnafiliaciones.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="3" cellStep="0" name="${lbl:b_affiliations}"/>
	
	<excel 
		recordset='query-smnafiliaciones.sql' 
		sheetname='${lbl:b_affiliations}' 
		filename='${lbl:b_affiliations}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='afc_codigo' label='${lbl:b_code}' />
		<col id='afc_descripcion' label='${lbl:b_description}' />
		<col id='ant_apellidos' label='${lbl:b_joined}' />
		<col id='mod_nombre' label='${lbl:b_module}' />
		<col id='afc_es_titular_combo' label='${lbl:b_titular}' />
		<col id='ptz_descripcion' label='${lbl:b_relationship}' />
		<col id='afc_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
