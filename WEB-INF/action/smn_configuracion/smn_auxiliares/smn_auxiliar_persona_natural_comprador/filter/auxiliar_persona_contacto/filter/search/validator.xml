<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='smn_clase_auxiliar_rf' type='integer' required='false' label='${lbl:b_class_auxiliary}' maxlen='10'/>
	<parameter id='smn_auxiliar_rf' type='integer' required='false' label='${lbl:b_auxiliary}' maxlen='10'/>
	<parameter id='apc_apellidos' type='varchar' required='false' label='${lbl:s_profile_lname}' maxlen='64'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='apc_apellidos' rule='like' />
	<parameter id='apc_nombres' type='varchar' required='false' label='${lbl:s_profile_fname}' maxlen='64'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='apc_nombres' rule='like' />
	<parameter id='apc_fecha_registro_ini' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
	<custom-validator classname='dinamica.validators.DateRangeValidator' on-error-label='[${lbl:b_store_date} mayor o igual que] no puede ser mayor que [${lbl:b_store_date} menor o igual que].' date1='apc_fecha_registro_ini' date2='apc_fecha_registro_fin' id='apc_fecha_registro_ini' />
	<parameter id='apc_fecha_registro_fin' type='date' required='false' label='${lbl:b_store_date}' maxlen='10'/>
</validator>
