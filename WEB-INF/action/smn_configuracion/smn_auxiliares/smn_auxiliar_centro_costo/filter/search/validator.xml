<?xml version='1.0' encoding='ISO-8859-1'?>
<validator id='filter.params' onerror='/action/error/validation/ajax'>
	<parameter id='tco_codigo' type='varchar' required='false' label='${lbl:b_code}' maxlen='16'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='tco_codigo' rule='contains' />
	<parameter id='tco_descripcion_larga' type='varchar' required='false' label='${lbl:b_long_description}' maxlen='48'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='tco_descripcion_larga' rule='contains' />
	<parameter id='tco_clase_auxiliar' type='integer' required='false' label='${lbl:b_auxiliary_class}' maxlen='10'/>
	<parameter id='tco_auxiliar' type='integer' required='false' label='${lbl:b_auxiliary}' maxlen='10'/>
	<parameter id='tco_responsable' type='varchar' required='false' label='${lbl:b_responsible}' maxlen='48'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='tco_responsable' rule='contains' />
	<parameter id='tco_tipo_cco' type='varchar' required='false' label='${lbl:b_type}' maxlen='2'/>
	<custom-validator classname='dinamica.validators.SQLPatternTransformer' parameter='tco_tipo_cco' rule='contains' />
</validator>
