<?xml version='1.0' encoding='ISO-8859-1'?>
<config nodb="true">

	<summary>
		Exportar documento excel directo al browser
	</summary>

	<log>false</log>

	<transaction>
		<classname>dinamica.GenericTransaction</classname>
		<validator>false</validator>
		<transaction>false</transaction>
		<jdbc-log>false</jdbc-log>
		<recordset id="query-smnagenda.sql" source="session" scope="transaction" />
	</transaction>

	<output>
		<classname>domain.ExcelReportOutput</classname>
	</output>
	
	<excel-logo scale="60" url="/images/logo-simone-pdf.png"/>
	<title cellPosY="1" cellPosX="4" cellStep="0" name="${lbl:b_timetable}"/>
	
	<excel 
		recordset='query-smnagenda.sql' 
		sheetname='${lbl:b_timetable}' 
		filename='${lbl:b_timetable}.xls' 
		date-format='dd-MM-yyyy'
		header-background='true'>
		<col id='age_codigo' label='${lbl:b_code}' />
		<col id='age_descripcion' label='${lbl:b_description}' />
		<col id='age_dia_semana_combo' label='${lbl:b_weekday}' />
		<col id='age_hora_desde' label='${lbl:b_time_since}' />
		<col id='age_jornada_desde_combo' label='${lbl:b_working_day}' />
		<col id='age_hora_hasta' label='${lbl:b_time_until}' />
		<col id='age_jornada_hasta_combo' label='${lbl:b_working_day}' />
		<col id='age_localidad' label='${lbl:b_location}' />
		<col id='age_fecha_registro' label='${lbl:b_store_date}' />

	</excel>

</config>
